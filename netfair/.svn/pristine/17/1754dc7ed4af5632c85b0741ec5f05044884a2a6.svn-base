		.new_search .sub_mit:hover{background: #31a2d6 url(/img/newindex/hbcompany05.png) 13px center no-repeat}
		 /*微信二维码*/
		.weiCodeTop{ text-align:center;width:138px; border:1px solid #dadada; background:#fff; padding-bottom:10px; position: absolute;top:130px; right:50%; margin-right:-650px;}
