        })

        $('.confirmComplete').on('click',function(){
            var selectReasons = [];
            var selectBox = $('input[name="notCompleteReason"]:checked')
            for(var i=0; i<selectBox.length; i++){
                var box = $(selectBox[i])
                var value = box.val()
                selectReasons.push(value)
            }

            if(selectReasons.length == 0){

