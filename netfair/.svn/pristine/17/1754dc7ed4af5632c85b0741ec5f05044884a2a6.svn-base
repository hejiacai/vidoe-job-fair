<body style="padding-bottom: 55px;background-color: #f6f7f8;">

<!-- <div class="resumeThead">
    <div class="resumeTop">
        <div>
            <a href="{/$homePage/}"></a>
            <span></span>
        </div>
    </div>
</div> -->
<div class="videoJobFairTop videoJobFairTop-resume">
    <div>
        <a>简历</a>
    </div>
</div>
<div class="resumeMain clearfix">
    <div class="sumeMainLeft">
        <input type="hidden" id="hddCurrResumeId" value="{/$resume_id/}" />
        <input type="hidden" id="hddapplyid" value="{/$applyid/}" />
        <input type="hidden" id="hddsrc" value="{/$src/}" />
		
        {/if $is_in_wait_rewait || ($is_not_call && $has_get_linkway)/}
        {/if $company_site==1/}
        <div class="netfair-RQcode clearfix">
            <img class="netfair-RQcode-img" src="{/get_url rule='/video/RtcScanCode' domain='company'/}?job_id={/$base_job_id/}&resume_id={/$base_resume_id/}&net_apply_id={/$apply_id/}"/>
            <p class="RQcode-title">汇博企业app扫码发起视频面试<span class="tipdownLoad">没有汇博APP？<a href="javascript:;" class="downLoadAppHb">点击下载</a></span></p>
        </div>
        {/elseif $company_site==2/}
        <div class="netfair-RQcode clearfix">
            <img class="netfair-RQcode-img" src="{/get_url rule='/videohall/RtcScanCode'/}?job_id={/$base_job_id/}&person_id={/$base_person_id/}&net_apply_id={/$apply_id/}"/>
            <p class="RQcode-title">快米企业app扫码发起视频面试<span class="tipdownLoad">没有快米APP？<a href="javascript:;" class="downLoadAppKm">点击下载</a></span></p>
        </div>
        {//if/}
        {//if/}


        {/if $remark_count > 0/}
        <!-- 展开操作记录 -->
        <div id="historyContainer" class="smNumberUp netfair-smNumberUp" style='display:block'>
            <a {/if $remark_count <= 0/}data-status="0"{/else/}data-status="1"{//if/} href="javascript:;" class="sberUp">收起操作记录<i class="icon-svg132"></i></a>
            <ul>
                {/foreach $remark_list as $key => $value/}
                <li class="remark_li" {/if $key>5/}style="display: none" {//if/}>
                <span>{/$value['update_time']/}</span>
                <em style="width: auto;">添加备注：{/$value['remark']/}</em>
                </li>
                {//foreach/}
                {/if count($remark_list)>6/}
                <li id="moreHistory" class="fot"><a href="javascript:" id="btnMoreHistory">查看更早记录</a></li>
                {//if/}
            </ul>
        </div>
        {//if/}



        <div class="smNumber">
            <span>简历编号：{/if $resume_site==1/}{/$base_resume_id/}(汇博){/else/}{/$base_person_id/}(快米){//if/}</span>{/if $is_from_not_read_list && $is_new_resume/}<img src="{/$siteurl.style/}/img/xiaoyuan/resume/resumeHot.png" >{//if/}
            <a style="display: none;" href="javascript:;">展开操作记录<i class="icon-svg122"></i></a>
            <span class="sberRt">最近登录：{/$last_login_time|date_format:'%Y.%m.%d'/}</span>
        </div>



        <!--/mj new end-->
        <div class="numberName" style="margin-bottom: 20px;">
            {/if !empty($avatar)/}
            <img class='nberImg' id="avatarImgBox" width="140" height="140" src="{/$avatar|replace:'http:':''/}" alt="" />
            {/else/}
            <img class='nberImg' width="140" height="140" src="{/$siteurl.style/}/img/v2/resumeM/head-defects.jpg" />
            {//if/}

            <div class="numberNx">
                <div class="nberN01">
                    <span style="height:37px;">{/$user_name/}</span>
                </div>

                <div class="nberN02" style='padding-top:0px;'>
                    {/if $sex!=''/}<span>{/get_name sex=$sex/}</span>{//if/}
                    {/if $age!=''/}<em>|</em>
                    <span>{/$age/}</span>{//if/}
                    {/if $degree!=''/}<em>|</em>
                    <span>{/get_name degree=$degree/}</span>{//if/}
                    {/if $work_year!=''/}<em>|</em>
                    <span>{/$work_year/}</span>{//if/}
                    {/if $stature!=''/}<em>|</em>
                    <span>{/$stature/}cm</span>{//if/}
                    {/if $avoirdupois!=''/}<em>|</em>
                    <span>{/$avoirdupois/}kg</span>{//if/}
                    {/*{/if $marriage!=''/}<em>|</em>*/}
                    {/*<span>{/get_name marriage=$marriage/}{/if $fertility!=''/}{/get_name fertility=$fertility/}{//if/}</span>{//if/}*/}
                    {/if $political_status!=''/}<em>|</em><span>{/get_name political=$political_status/}</span>{//if/}
                    <!-- @update 新增贫困地区 2019-5-16-->
                    {/if $_person_exp['is_poor_family']/}
                    <!--是贫困家庭-->
                    <em>|</em><span class="poor">贫困建档家庭</span>
                    {/else/}
                    <!--不是贫困家庭-->
                    {//if/}

                </div>

                <div class="nberN03">
                    {/if $native_place_name!=''/}<span>户籍：</span>{/$native_place_name/}{//if/}
                    <!-- @update 新增贫困地区 2019-5-16-->
                    {/if $_person_exp['is_poor_family']/}
                    <!--是贫困家庭-->
                    (<span class="poor-place">{/$_person_exp['family_address']/}</span>)&nbsp;&nbsp;&nbsp;&nbsp;
                    {/else/}
                    <!--不是贫困家庭-->
                    {//if/}
                    {/if $cur_area_name!=''/}<span>现居：</span>{/$cur_area_name/}{/else/}&nbsp;{//if/}
                </div>

                <div id="linkwayContainer" class="nberN04">
                    {/if $show_linkway/}
                    {/if !empty($mobile_phone)/}
                    <em></em><span id="spnBasicMobilePhone" data-phone="{/$mobile_phone/}">{/if $has_get_linkway/}{/$mobile_phone/}{/else/}{/$hid_mobile_phone/}{//if/}</span><span id="phoneAddr"></span>{//if/}

                    {/if !empty($email)/}<i></i><span class="show_email" data-email="{/$email/}">{/if $has_get_linkway/}{/$email/}{/else/}{/$hid_email/}{//if/}</span>{//if/}

                    {/if !$has_get_linkway/}
                    {/if !empty($qq)/}<b style="display:none" class="hidQQ"></b><span class="hidQQ" style="display:none">{/$qq/}</span>{//if/}
                    <!-- <div id="see_mobile">免费查看联系方式</div> -->
                    {/else/}
                    {/if !empty($qq)/}<b></b><span>{/$qq/}</span>{//if/}
                    {//if/}

                    {/else/}

                    {/if !$has_get_linkway/}
                    {/if $is_wait_interview/}
                    <p style="color: red">（面试完视频大厅待面试求职者后，可查看联系方式）</p>
                    {/else/}
                    <!--<p id="linkWayBar"   class="nolink"><a href="javascript:void(0);" id="btnGetLinkway">立即获取联系方式</a></p>-->
                    {//if/}
                    {//if/}

                    {/if !empty($mobile_phone)/}<em></em>
                    <span id="spnBasicMobilePhone" {/if $mobile_phone==''/}style="display:none"{//if/}>{/$mobile_phone/}</span><span id="phoneAddr"  {/if !$has_get_linkway/}style="display:none"{//if/}></span>{//if/}
                    {/if !empty($email)/}<i></i><span>{/$email/}</span>{//if/}
                    {/if !empty($qq)/}<b></b><span>{/$qq/}</span>{//if/}
                    {//if/}
                </div>


            </div>
        </div>


        {/if count($video_list)>0/}
        <div class="resumeVideoTit">
            <span>该求职者精心准备了<b>短视频简历</b>，如果对其感兴趣可滑到简历底部观看 !</span>
        </div>
        {//if/}

        {/if $exp_job || $exp_area_names || $str_expect_callings || $str_expect_jobsorts || $job_state_text/}
        <div class="sumeNtit">
            <em class="titIcon01"></em>
            求职意向
        </div>
        <ul class="nberN05">
            {/if $exp_job/}
            <li>
                <b>希望从事：</b>
                <p>{/$exp_job/}
                    <em>{/if $job_type==1&&$is_accept_parttime==1/}（可以接受兼职）{/elseif $job_type==2/}（兼职）{/elseif $job_type==3/}（实习）{//if/}</em>
                    {/if $exp_job_level!=''/}<i>|</i>{/get_name job_level=$exp_job_level/}{//if/}
                    {/if $exp_salary/}<i>|</i>
                    {/if $exp_salary=='negotiable'/}面议{/else/}{/$exp_salary/} 元/月{//if/}
                    {/else/}<i>|</i>面议{//if/}
                    {/if $is_not_accept_lower_salary==1&&$exp_salary!='negotiable'&&$exp_salary!='01'/}<em>（不低于此薪资）</em>
                    {//if/}
                </p>
            </li>
            {//if/}

            {/if $exp_area_names/}
            <li>
                <b>工作地点：</b>
                <p>{/$exp_area_names/}</p>
            </li>
            {//if/}

            {/if $str_expect_callings/}
            <li>
                <b>期望行业：</b>
                <p>{/$str_expect_callings/}</p>
            </li>
            {//if/}

            {/if $str_expect_jobsorts/}
            <li>
                <b>期望类别：</b>
                <p>{/$str_expect_jobsorts/}</p>
            </li>
            {//if/}

            {/if $job_state_text/}
            <li>
                <b>求职状态：</b>
                <p>{/get_name apply_status=$job_state_id/}{/if $accession_time/}<i>|</i>{/get_name accession_time=$accession_time/}{//if/}</p>
            </li>
            {//if/}
        </ul>
        {//if/}



        {/if !base_lib_BaseUtils::nullOrEmpty($appraise)/}
        <style>
            .nberN06 p{text-indent:30px}
        </style>
        <div class="sumeNtit appraiseContent">
            <em class="titIcon02"></em>
            自我评价
        </div>
        <div class="nberN06"  style="overflow: hidden; text-indent:30px">{/html_encode content=$appraise/}</div>
        {//if/}

        {/if count($resume_works) > 0/}
        <div class="sumeNtit">
            <em class="titIcon03"></em>
            工作/实习经历
        </div>
        {/foreach $resume_works as $key => $value/}
        <div class="nberN07 nberN11 {/if $value@last/}nberN11last{//if/}">
            <p class="nberTit1">{/$value['start_time']/}-{/$value['end_time']/}<span>{/if $value['work_time']/}[{/$value['work_time']/}]{//if/} {/get_name job_type=$value['job_type']/} {/if $value['is_creator']=='1'/}[创始人]{//if/}</span></p>
            <p class="nberTit2"><b>{/$value['station']/}</b>
                {/if $value['department']/}<span>({/$value['department']/})</span>{//if/}
                {/if !empty($value['job_level'])/}<span> {/get_name job_level=$value['job_level']/}</span>{//if/}
                {/if $value['is_salary_show'] != '1' && $value['salary_month']/}<i>|</i><span>{/$value['salary_month']/}元/月</span>{//if/}
            </p>

            <p class="nberTit3"><b>{/$value['company_name']/}</b>
                {/if (!empty($value['calling_id']))/}
                <span>{/get_name calling=$value['calling_id']/}</span>
                {//if/}
                {/if (!empty($value['com_property']))/}
                <i>|</i><span>{/get_name com_property=$value['com_property']/}</span>
                {//if/}
                {/if (!empty($value['rewards']))/}<i>|</i><span>$value['rewards']</span>{//if/}
                {/if (!empty($value['com_size']))/}<i>|</i><span>{/get_name com_size=$value['com_size']/}</span></p>{//if/}

            {/if $value['tag_names']/}
            <div class="jobTags">
                {/foreach $value['tag_names'] as $tag_name/}
                <em>{/$tag_name/}</em>
                {//foreach/}
            </div>
            {//if/}
            <ul class="nberN05 nberN05x" style="margin:0;">
                {/if !empty($value['work_content'])/}
                <li>
                    <b>工作职责：</b>
                    <div>
                        <p>{/html_encode content=$value['work_content']/}</p>
                    </div>
                </li>
                {//if/}

            </ul>
        </div>
        {//foreach/}
        {//if/}



        {/if count($resume_edus) > 0 || count($resume_trains) > 0/}
        <div class="sumeNtit" style="margin-top:20px;">
            <em class="titIcon04"></em>
            教育/培训经历
        </div>
        <div class="nberN08" style="margin-bottom:20px">
            {/foreach $resume_edus as $key=>$value/}
            <div class="nber8bg {/if $value@last && empty($resume_trains)/}nber8Last{//if/}" style=" padding:10px 0;">
                {/if !empty($value['start_time']) || !empty($value['end_time'])/}<p class="nberTit1">{/if empty($value['start_time'])/}开始时间未填写{/else/} {/$value['start_time']|date_format:'%Y.%m'/}{//if/}-{/if empty($value['end_time'])/}至今{/else/}{/$value['end_time']|date_format:'%Y.%m'/}{//if/}</p>{//if/}
                <p class="nberTit2">{/if $value['degree']!=''/}<b>{/get_name degree=$value['degree']/}</b>{//if/}{/if $value['school']!=''/}<i>|</i><span>{/$value['school']/}</span>{//if/}{/if $value['major_desc']!=''/}<i>|</i><span>{/$value['major_desc']/}</span>{//if/}</p>
                <ul class="nberN05 nberN05x" style="margin:0;">
                    {/if $value['school_score']!=''/}
                    <li>
                        <b>在校成绩：</b>
                        <div>
                            <p>{/$value['school_score']/}</p>
                        </div>
                    </li>
                    {//if/}
                    {/if $value['duty']!=''/}
                    <li>
                        <b>校内职务：</b>
                        <div>
                            <p>{/$value['duty']/}</p>
                        </div>
                    </li>
                    {//if/}
                    {/if $value['edu_detail']!=''/}<li>
                    <b>专业课程：</b>
                    <div>
                        <p>{/html_encode content=$value['edu_detail']/}</p>
                    </div>
                </li>
                    {//if/}
                </ul>
            </div>
            {//foreach/}
            {/foreach $resume_trains as $key=>$value/}
            <div class="nber8bg {/if $value@last/}nber8Last{//if/}">
                {/if !empty($value['start_time']) || !empty($value['end_time'])/}<p class="nberTit1">{/if empty($value['start_time'])/}开始时间未填写{/else/} {/$value['start_time']|date_format:'%Y.%m'/}{//if/}-{/if empty($value['end_time'])/}至今{/else/}{/$value['end_time']|date_format:'%Y.%m'/}{//if/}<span>培训</span></p>{//if/}
                <p class="nberTit2">{/if $value['course']!=''/}<b>{/$value['course']/} </b>{//if/}{/if $value['institution']!=''/}<span>{/$value['institution']/}</span>{//if/}{/if $value['certificate']!=''/}<i>|</i><span>{/$value['certificate']/}</span>{//if/}</p>
            </div>
            {//foreach/}
        </div>
        {//if/}

        {/if count($resume_winningrecord) > 0/}
        <div class="sumeNtit">
            <em class="titIcon14"></em>
            获奖经历
        </div>
        <div class="nberN07">
            <ul class="nberN05 nberN05x" style="margin:0;">
                {/foreach $resume_winningrecord as $key => $value/}
                <li>
                    <b>{/$value['winning_time']/} </b>
                    <div>
                        <p>{/$value['winning_name']/}
                            {/if $value['winning_desc']/}
                            ({/$value['winning_desc']/})
                            {//if/}
                        </p>
                    </div>
                </li>
                {//foreach/}
            </ul>
        </div>
        {//if/}

        {/if $isshowresumeinfo/}
        {/if count($resume_projects) > 0/}
        <div class="sumeNtit" style="width: 180px">
            <em class="titIcon05"></em>
            校内外项目经验
        </div>
        <div class="nberN08" style="margin-bottom:20px">
            {/foreach $resume_projects as $key => $value/}
            <div class="nber8bg{/if $value@last/} nber8Last{//if/}" style=" padding:10px 0;">
                {/if !empty({/$value['start_time']/}) || !empty({/$value['end_time']/})/}<p class="nberTit1">{/if empty($value['start_time'])/}开始时间未填写{/else/} {/$value['start_time']/}{//if/}-{/if empty($value['end_time'])/}至今{/else/}{/$value['end_time']/}{//if/}</p>{//if/}
                <p class="nberTit2">{/if $value['project_name']!=''/}<b>{/$value['project_name']/}</b>{//if/}{/if $value['duty']!=''/}<span class="orange">担任：{/$value['duty']/}</span>{//if/}</p>
                <ul class="nberN05 nberN05x" style="margin:0;">
                    {/if $value['project_detail'] != ''/}
                    <li>
                        <b>项目介绍：</b>
                        <div>
                            <p>{/$value['project_detail']/}</p>
                        </div>
                    </li>
                    {//if/}
