<style type="text/css">
.jobErr .bd{padding:75px 0 75px;margin-bottom:30px; zoom:1;}
.jobErr .bd .l{float:left;display:inline; text-align:right;width:250px;}
.jobErr .bd .r{float:left;display:inline;width:480px;margin-left:15px;}
.jobErr .bd .l i.hbFntWes{font-size:36px;color:#f9ae0c}
.jobErr .bd .r p{margin-bottom:5px;font-size:14px;color:#666;}
.jobErr .bd .r p.tit{font-size:20px;color:#f9ae0c;}
.jobErr .bd .r p.txt a{padding-right:10px;}
 dl{margin-top:10px;}
 dl dt{font-weight:bold; height:26px; line-height:26px;}
 dl dd .photoImg{width:61px; height:76px; float:left; display:inline; background:url({/$siteurl.style/}/img/common/user61_76.jpg) no-repeat;}
 dl dd .photoImg img{width:61px; height:76px;}
 dl dd .txt{float:left; display:inline; margin-left:10px;}
 .r .txt1{margin-left:70px;}
</style>
