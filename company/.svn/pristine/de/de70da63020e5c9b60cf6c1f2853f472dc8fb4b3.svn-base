	function addGray(cur,total){
		if(cur==1){
			pageR.addClass('gray')
		}else{
		    pageR.removeClass('gray')
		}
		if(cur>=total){
			pageL.addClass('gray')
		}else{
			pageL.removeClass('gray')
		}
	}
