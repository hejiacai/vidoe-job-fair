function refreshReward(defaultReward,defaultRewardName,otherReward){
    $("#hidDefaultReward").val(defaultReward);
    $("#hidOtherReward").val(otherReward);
    //重新展示页面
    var reward_html = '';
    var defaultReward_arr = [];
    var otherReward_arr = [];
    if(defaultRewardName != ''){
        defaultReward_arr =defaultRewardName.split(',');
    }
    if(otherReward !=''){
        otherReward_arr = otherReward.split(',');
    }
    for(var i=0;i<defaultReward_arr.length;i++){
        reward_html = reward_html+"<span class='flitem'>"+defaultReward_arr[i]+"</span>";
    }
    for(var k=0;k<otherReward_arr.length;k++){
        reward_html = reward_html+"<span class='flitem'>"+otherReward_arr[k]+"</span>";
    }
    reward_html = reward_html+ '  <a href="javascript:void(0)" onclick="setWelfare(\''+defaultReward+'\',\''+otherReward+'\')">编辑</a>';
    $("#selectedReward").html(reward_html);
//    <span class="formChb flitem">福利待遇1</span> 
//                            <a href="javascript:void(0)" onclick="setWelfare(0,0)">编辑</a>
}
