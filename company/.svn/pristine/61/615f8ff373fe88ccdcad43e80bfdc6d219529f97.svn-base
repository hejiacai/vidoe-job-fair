function get_mien_infor(){
    var mien_infor = {"has_data":false};
    var $mien_infor_form = $("#mien");
    var path = $mien_infor_form.find("input[name=video_path]").val();
    if(path != ""&&path!="优酷、土豆等网站的视频url地址"){
        mien_infor.has_data = true;
        mien_infor.mien_name = $mien_infor_form.find("input[name=video_name]").val();
        mien_infor.mien_path = $mien_infor_form.find("input[name=video_path]").val();
    }
    return mien_infor;
}
