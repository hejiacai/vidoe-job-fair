hbjs.use('@confirmBox', function(m) {	
	
	var ConfirmBox = m['widge.overlay.confirmBox'],
		Dialog = m['widge.overlay.hbDialog'],
		cookie = m['tools.cookie'],
		fontSize = 18,
		pWidth = 70;
	
	infoNoComplete = new Dialog({
	    close: '╳',
	    idName: 'buyPoints',
	    width: 400,
	    title: '简历不详细',
	    content: $('.notCompletePop')
	});
	

	$('.remindUser').on('click',function(){
		$('input[name="notCompleteReason"]').attr('checked',false)
		infoNoComplete.show()
	})
	
	
	$('.cancelComplete').on('click',function(){
		infoNoComplete.hide()
	})
	
	$('.confirmComplete').on('click',function(){
		var selectReasons = [];
		var selectBox = $('input[name="notCompleteReason"]:checked')
		for(var i=0; i<selectBox.length; i++){
			var box = $(selectBox[i])
			var value = box.val()
			selectReasons.push(value)
		}
		
		if(selectReasons.length == 0){
			
			ConfirmBox.timeBomb('请选择不详细原因', {
				name: 'fail',
				width: 200,
				timeout: 2000
			});
		}else{
            $.post('/resume/ResumeAlertRemind',{resume_id:'{/$resume_id/}',remind_tpl:selectReasons.join(',')},function (e) {
                log_data.remain_tpl = selectReasons.join(',');
                action_dom = [
                    ['.remain_resume', 68, log_data],//对应码表参数
                ];
                $('.remain_resume').click();
                infoNoComplete.hide()
								
								ConfirmBox.timeBomb('反馈成功', {
									name: 'success',
									width: 150,
									timeout: 2000
								});
								
								
            },'json')
		}
	})
});
