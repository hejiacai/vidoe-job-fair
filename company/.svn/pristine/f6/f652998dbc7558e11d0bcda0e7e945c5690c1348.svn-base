<style type="text/css">
.subSta{display:inline-block;width:78px; height:13px;}
.subSta00{ background:url({/$siteurl.style/}/img/pcstaricon.png) 0 0 no-repeat;}
.subSta01{ background:url({/$siteurl.style/}/img/pcstaricon.png) 0 -13px no-repeat;}
.subSta02{ background:url({/$siteurl.style/}/img/pcstaricon.png) 0 -26px no-repeat;}
.subSta03{ background:url({/$siteurl.style/}/img/pcstaricon.png) 0 -39px no-repeat;}
.subSta04{ background:url({/$siteurl.style/}/img/pcstaricon.png) 0 -52px no-repeat;}
.subSta05{ background:url({/$siteurl.style/}/img/pcstaricon.png) 0 -65px no-repeat;}

/*======新加新式======*/
.resumeNewtop{width:100%; height:78px; border-radius:12px 12px 0 0; background:#f5fbff; border-bottom:1px solid #e1f0fa;}
.resumeNtoplf{ float:left; color:#6699cc; font-size:16px; line-height:78px; padding-left:30px; font-family:"微软雅黑";}
.resumeNtoprt{ float:right; padding-right:30px;text-align:right}  
.resumeNtoprt p{ display:block; margin-top:20px; margin-bottom:2px;}
.resumeNtoprt p span,.resumeNtoprt p a{ display:inline-block; vertical-align:middle; line-height:36px; font-family:"微软雅黑"; font-size:14px;}
.resumeNtoprt p a{cursor:default; width:130px; height:36px; text-align:center; font-size:16px; border-radius:4px; margin-left:8px;}
.resumeNtoprt p a:hover{ color:#fff;}
.resumeNbtn{ display:block; background:url({/$siteurl.style/}/img/c/resumeNbtn.png) 0 center no-repeat; padding-left:20px; color:#63a0ca; font-size:12px; font-family:"宋体"; line-height:20px; margin-left:228px;margin-right:26px;}
.resumeNtoprtx p{ height:20px; line-height:20px; text-align:right;}
.resumeNtoprtx p span{ line-height:20px; color:#333;}
.resumeFit{width:153px; overflow:hidden; text-align:right; position:absolute; top:23px; right:67px; z-index:5;}
.resumeFit a{ display:inline-block;width:125px; height:36px; color:#fff; text-align:center; font-size:16px; font-family:"微软雅黑"; line-height:36px; border-radius:0px 4px 4px 0px;}
.resumeFit img{ display:block;}
.resumeFit a.resumeFitbtn01{ background:#47bf8e; margin-bottom:6px;}
.resumeFit a.resumeFitbtn02{ background:#f36861;}
.resumeFit a.resumeFitbtn01:hover{ background:#4bcc96;}
.resumeFit a.resumeFitbtn02:hover{ background:#d95d57;}

/*.resume_status0,.resume_status1,.resume_status2,.resume_status3,.resume_status4,.resume_status5,.resume_status6,.resume_status7,.resume_status8,.resume_status9,.resume_status99{ background:#ccc;color:#fff;}
.resume_status1,.resume_status3,.resume_status3,.resume_status3,.resume_status7,.resume_status9{ background:#1ecdab;}
.resume_status2,.resume_status4,.resume_status5,.resume_status6,.resume_status8{ background:#9eaeb2;}
.resume_status0{background:#f79a16;}
.resume_status99{background:#ff6b50;}*/
.infor-box .name span i{display:block;}

.resFixt{ margin-left:20px; border-bottom:1px dashed #ccc; padding-bottom:8px; padding-top: 15px; width: 760px;}
.resFixt b{ color:#09c; font-size:14px; padding-right:12px;}
.resFixt span{color:#09c;}

/*======顶部提示窗======*/
.weChatip{ overflow:hidden;width:440px; text-align:left;position:absolute;}
.weChatip img{ display:block; margin-left:50px; position:absolute;}
.subWtip01{width:94%; padding:9px; border:1px solid #ffe4b5; background:#fefce8; overflow:hidden; margin-top:7px;}
.subWtip02{ float:left;width:400px; color:#e5a408;}
.subWtip02 a{ color:#3d84b8;}
.subWtip02 span{ color:#f00;}
.subWtip02 a:hover{ text-decoration:underline;}
.wChatBtn{ display:block; float:right; width:7px; height:7px; background:url({/$siteurl.style/}/img/c/firm02.png) no-repeat; margin-top:4px;}
.resFixt{ margin-left:20px; margin-bottom: 20px; border-bottom:1px dashed #ccc; padding-bottom:8px; padding-top: 15px; width: 760px;}
.resFixt b{ color:#09c; font-size:14px; padding-right:12px;}
.resFixt span{color:#09c;}

.smNumberUp .blur{color: #3d84b8;}
.smNumberUp .gray3{color: #333;}
.formRad label{margin:0;}

.yaoqing-alert {
    margin: 30px;
    color: #999;
    font-size: 14px;
    line-height: 22px;
    text-align: center;
}
.yaoqing-alert .t2 {
    margin-bottom: 15px;
}
.yaoqing-alert .t2 span {
    color: #ff6563;
}
.yqQual {
    height: 30px;
    background: #66bce4;
    font-size: 14px;
    color: #fff;
    line-height: 30px;
    border-radius: 2px;
    margin-right: 10px;
    width: 121px;
    display: inline-block;
}
.yqQual:hover{ background: #31a2d6; color: #fff;}

.materBox{
	padding:10px 50px;
	background: #fff6ec url({/$siteurl.style/}/img/orangebubble.jpg) no-repeat 20px center/20px 20px;
	    color: #ff8400;
}
.resumeMain{background: #FFFFFF;}
html,body{width: 870px;overflow-y: auto;overflow-x: hidden;min-height: 654px;background: #FFFFFF;}
.sumeMainLeft{border: 0;}
</style>
</head>
<body>
		<!--主体-->
		<div class="resumeMain clearfix" style="width:870px; overflow:hidden; margin:0;">
			<!--左边-->			
            <div class="sumeMainLeft" style="width:870px">
               
              <input type="hidden" id="hddCurrResumeId" value="{/$resume_id/}" />
		      <input type="hidden" id="hddapplyid" value="{/$applyid/}" />
		      <input type="hidden" id="hddsrc" value="{/$src/}" />                    
    <div class="sumeMainLeft" style="width:870px">
    	<div class="smNumber">
        	<span>简历编号：{/$resume_id/}</span>
        	{/if count($historyrecords)>0||count($historyrecordagos)>0/}<a style='display:none' href="javascript:;">展开操作记录<i></i></a>{//if/}
            <span class="sberRt">最近登录：{/$last_login_time|date_format:'%Y-%m-%d'/}</span>
        </div>
        <!--mj new-->
        <style>
        	.attest_alert{height: 40px;line-height: 40px;background-color: #ffefd5;color:#eb6f0b;font-size:14px;margin:10px 20px;padding:0 15px}
        	.attest_alert a{float:right}
        </style>
        {/if !$isshowresumeinfo/}<div class="attest_alert">{/$letter_info['msg']/}</div>{//if/}
        <!--/mj new end-->
        <div class="numberName">
        	{/if !empty($avatar)/}<img width="140" height="140" id="avatarImgBox" src="{/$avatar/}" alt="" />{/else/}<img class='nberImg' width="140" height="140" src="{/$siteurl.style/}/img/v2/resumeM/head-defects.jpg" />{//if/}
            <div class="numberNx">
            	<div class="nberN01">
                	<span>{/$user_name/}</span>
                    {/if !$isCqNewService/}
                        <em class="subSta subSta0{/if !empty($resumelevel)/}{/$resumelevel['resume_level_name']-1/}{/else/}0{//if/}"></em>
                        {/if !$show_linkway/}<b>（{/$resumelevel['resume_level_name']/}简历需要{/$resumelevel['point']/}个下载点）</b>{//if/}
                    {//if/}
                </div>
                <div class="nberN02">
                	{/if $sex!=''/}<span>{/get_name sex=$sex/}</span>{//if/}
                    {/if $age!=''/}<em>|</em>
                    <span>{/$age/}</span>{//if/}
                    {/if $degree!=''/}<em>|</em>
                    <span>{/get_name degree=$degree/}</span>{//if/}
                    {/if $work_year!=''/}<em>|</em>
                    <span>{/$work_year/}</span>{//if/}
                    {/if $stature!=''/}<em>|</em>
                    <span>{/$stature/}cm</span>{//if/}
                    {/if $avoirdupois!=''/}<em>|</em>
                    <span>{/$avoirdupois/}kg</span>{//if/}
                    {/if $marriage!=''/}<em>|</em>
                    <span>{/get_name marriage=$marriage/}{/if $fertility!=''/}{/get_name fertility=$fertility/}{//if/}</span>{//if/}
                    {/if $political_status!=''/}<em>|</em>
                    <span>{/get_name political=$political_status/}</span>{//if/}
                </div>
                <div class="nberN03">
                	{/if $native_place_name!=''/}<span>户籍：</span>{/$native_place_name/}&nbsp;&nbsp;&nbsp;&nbsp;{//if/}
                    {/if $cur_area_name!=''/}<span>现居：</span>{/$cur_area_name/}{//if/}
                </div>
                <div id="linkwayContainer" class="nberN04">
                	{/if $show_linkway==true/}
	                	{/if !empty($mobile_phone)/}<em></em>
	                    <span id="spnBasicMobilePhone">{/$mobile_phone/}</span><span id="phoneAddr"></span>{//if/}
	                    {/if !empty($email)/}<i></i>
	                    <span>{/$email/}</span>{//if/}	                    
	                    {/if !empty($qq)/}<b></b><span>{/$qq/}</span>{//if/}
		            {/else/}
		            	<p id="linkWayBar" style='display:none;'  class="nolink"><a href="javascript:void(0);" id="btnGetLinkway">立即获取联系方式</a>
		            		<span style="color:#333; font-family:'宋体';font-size:12px;font-weight: normal;">（{/if !empty($resumelevel)/}{/$resumelevel['resume_level_name']/}{/else/}1星{//if/}简历需要{/if !empty($resumelevel)/}{/$resumelevel['point']/}{/else/}1{//if/}个下载点）</span>	
		            	</p>
                        {/if !empty($mobile_phone)/}<em></em>
	                    <span id="spnBasicMobilePhone" {/if $mobile_phone==''/}style="display:none"{//if/}>{/$mobile_phone/}</span><span id="phoneAddr"  {/if !$show_link/}style="display:none"{//if/}></span>{//if/}
	                    {/if !empty($email)/}<i></i>
	                    <span>{/$email/}</span>{//if/}	                    
	                    {/if !empty($qq)/}<b></b>
	                    <span>{/$qq/}</span>{//if/}
		            {//if/}
                </div>
            </div>
        </div>
        {/if $exp_job || $exp_area_names || $str_expect_callings || $str_expect_jobsorts || $job_state_text/}
        <div class="sumeNtit">
        	<em class="titIcon01"></em>
            求职意向
        </div>
        {//if/}
        <ul class="nberN05">
        	{/if $exp_job/}<li>
            	<b>希望从事：</b>
                <p>{/$exp_job/}
                	<em>{/if $job_type==1&&$is_accept_parttime==1/}（可以接受兼职）{/elseif $job_type==2/}（兼职）{/elseif $job_type==3/}（实习）{//if/}</em>
                    {/if $exp_job_level!=''/}<i>|</i>{/get_name job_level=$exp_job_level/}{//if/}
                    {/if $exp_salary!=''||$exp_salary!='0'/}<i>|</i>
                    {/if $exp_salary=='negotiable'/}面议{/else/}{/get_name salary=$exp_salary/} 元/月{//if/}
                    {/else/}<i>|</i>面议{//if/}{/if $is_not_accept_lower_salary==1&&$exp_salary!='negotiable'&&$exp_salary!='01'/}<em>（不低于此薪资）</em>{//if/}
                </p>
            </li>{//if/}
            {/if $exp_area_names/}<li>
            	<b>工作地点：</b>
                <p>{/$exp_area_names/}</p>
            </li>{//if/}
            {/if $str_expect_callings/}<li>
            	<b>期望行业：</b>
                <p>{/$str_expect_callings/}</p>
            </li>{//if/}
            {/if $str_expect_jobsorts/}<li>
            	<b>期望类别：</b>
                <p>{/$str_expect_jobsorts/}</p>
            </li>{//if/}
            {/if $job_state_text/}
            <li>
            	<b>求职状态：</b>
                <p>{/get_name apply_status=$job_state_id/}{/if $accession_time/}<i>|</i>{/get_name accession_time=$accession_time/}{//if/}</p>
            </li>
            {//if/}
        </ul>
        {/if !base_lib_BaseUtils::nullOrEmpty($appraise)/}
        <div class="sumeNtit">
        	<em class="titIcon02"></em>
            自我评价
        </div>
        <div class="nberN06" style="width:610px">{/$appraise/}</div>
        {//if/}
        {/if count($resume_works)>0/}
        <div class="sumeNtit">
        	<em class="titIcon03"></em>
            工作经历
        </div>
        {/foreach $resume_works as $key=>$value/}
        <div class="nberN07 nberN11 {/if $value@last/}nberN11last{//if/}">
        	<p class="nberTit1">{/$value['start_time']/}-{/$value['end_time']/}<span>{/if $value['work_time']/}[{/$value['work_time']/}]{//if/} {/get_name job_type=$value['job_type']/} {/if $value['is_creator']=='1'/}[创始人]{//if/}</span></p>
            <p class="nberTit2"><b>{/$value['station']/}</b>{/if !empty($value['job_level'])/}<span> {/get_name job_level=$value['job_level']/}</span>{//if/}{/if $value['is_salary_show'] != '1' && $value['salary_month']/}<i>|</i><span>{/$value['salary_month']/}元/月</span>{//if/}</p>
            <p class="nberTit3"><b>{/$value['company_name']/}</b>
            {/if (!empty($value['com_property']))/}
              <span>{/get_name com_property=$value['com_property']/}</span>
            {//if/}
            {/if (!empty($value['rewards']))/}<i>|</i><span>$value['rewards']</span>{//if/}
            {/if (!empty($value['com_size']))/}<i>|</i><span>{/get_name com_size=$value['com_size']/}</span></p>{//if/}
            <ul class="nberN05" style="margin:0;">
        	{/if !empty($value['work_content'])/}
        	<li>
            	<b>工作职责：</b>
                <p>{/html_encode content=$value['work_content']/}</p>
            </li>
            {//if/}
            {/if !empty($value['leave_reason'])/}
            <li>
            	<b>离职原因：</b>
                <p>{/$value['leave_reason']/}</p>
            </li>
            {//if/}
        </ul>
        </div>
        {//foreach/}
        {//if/}
        {/if count($resume_edus)>0||count($resume_trains)>0/}
        <div class="sumeNtit">
        	<em class="titIcon04"></em>
            教育培训
        </div>
        <div class="nberN08">
        {/foreach $resume_edus as $key=>$value/}
        	<div class="nber8bg {/if $value@last/}nber8Last{//if/}">
            	{/if !empty($value['start_time']) || !empty($value['end_time'])/}<p class="nberTit1">{/if empty($value['start_time'])/}开始时间未填写{/else/} {/$value['start_time']|date_format:'%Y.%m'/}{//if/}-{/if empty($value['end_time'])/}至今{/else/}{/$value['end_time']|date_format:'%Y.%m'/}{//if/}</p>{//if/}
                <p class="nberTit2">{/if $value['degree']!=''/}<b>{/get_name degree=$value['degree']/}</b>{//if/}{/if $value['school']!=''/}<i>|</i><span>{/$value['school']/}</span>{//if/}{/if $value['major_desc']!=''/}<i>|</i><span>{/$value['major_desc']/}</span>{//if/}</p>
                <ul class="nberN05" style="margin:0;">
                    {/if $value['duty']!=''/}<li>
                        <b>校内职务：</b>
                        <p>{/$value['duty']/}</p>
                     </li>
                     {//if/}
                     {/if $value['edu_detail']!=''/}<li>
                        <b>专业课程：</b>
                        <p>{/$value['edu_detail']/}</p>
                     </li>
                     {//if/}
             	</ul>
            </div>
         {//foreach/}
         {/foreach $resume_trains as $key=>$value/}
            <div class="nber8bg {/if $value@last/}nber8Last{//if/}">
            	{/if !empty($value['start_time']) || !empty($value['end_time'])/}<p class="nberTit1">{/if empty($value['start_time'])/}开始时间未填写{/else/} {/$value['start_time']|date_format:'%Y.%m'/}{//if/}-{/if empty($value['end_time'])/}至今{/else/}{/$value['end_time']|date_format:'%Y.%m'/}{//if/}<span>培训</span></p>{//if/}
                <p class="nberTit2">{/if $value['course']!=''/}<b>{/$value['course']/} </b>{//if/}{/if $value['institution']!=''/}<span>{/$value['institution']/}</span>{//if/}{/if $value['certificate']!=''/}<i>|</i><span>{/$value['certificate']/}</span>{//if/}</p>
             </div>
         {//foreach/}
        </div>
        {//if/}
    {/if $isshowresumeinfo/}
        {/if count($resume_projects)>0/}
        <div class="sumeNtit">
        	<em class="titIcon05"></em>
            项目经验
        </div>
        <div class="nberN08">
        {/foreach $resume_projects as $key=>$value/}
        	<div class="nber8bg{/if $value@last/} nber8Last{//if/}">
            	{/if !empty({/$value['start_time']/}) || !empty({/$value['end_time']/})/}<p class="nberTit1">{/if empty($value['start_time'])/}开始时间未填写{/else/} {/$value['start_time']/}{//if/}-{/if empty($value['end_time'])/}至今{/else/}{/$value['end_time']/}{//if/}</p>{//if/}
                <p class="nberTit2">{/if $value['project_name']!=''/}<b>{/$value['project_name']/}</b>{//if/}{/if $value['duty']!=''/}<span class="orange">担任：{/$value['duty']/}</span>{//if/}</p>
                <ul class="nberN05" style="margin:0;">
                    {/if $value['project_detail']!=''/}<li>
                        <b>项目介绍：</b>
                        <p>{/$value['project_detail']/}</p>
                     </li>{//if/}
                     {/if $value['main_duty']!=''/}<li>
                        <b>项目经历：</b>
                        <p>{/$value['main_duty']/}</p>
                     </li>{//if/}
             	</ul>
            </div>
         {//foreach/}
        </div>
        {//if/}
    {/else/}
		<!--项目经验-->
		<div class="sumeNtit">
        	<em class="titIcon05"></em>
            项目经验
        </div>
		<div class="nberN08">
			<div class="nber8bg nber8Last">
				<!--没数据时显示--><p class="nberTit2"><b>您暂未通过企业认证，认证后即可查看完整简历</b></p>
			</div>
		</div>
		<!--/-->	
	{//if/}
        {/if count($resume_languages)>0/}
        <div class="sumeNtit">
        	<em class="titIcon06"></em>
            语言能力
        </div>
        <div class="nberN07">
        {/foreach $resume_languages as $key=>$value/}
        	<p class="nberTit2 nberTit2x"><b>{/get_name language=$value['language_type']/}</b>{/if $value['skill_level']!=''/}<span>{/get_name skill_level=$value['skill_level']/}</span>{//if/}
        		{/if $value['certificates'][0]['cert_name']!=''/}
                  <i>|</i>
                  <span>
                  {/section name=language loop=$value['certificates']/}
                  	{/if $smarty.section.language.last/}
                 			{/$value['certificates'][language]['cert_name']/}
                 		{/else/}
                 			{/$value['certificates'][language]['cert_name']/}，	
                 		{//if/}
                 	{//section/}
                  </span>
                {//if/}
		    </p>
        {//foreach/}
        </div>
        {//if/}
        {/if count($resume_skills)>0/}
        <div class="sumeNtit">
        	<em class="titIcon07"></em>
            技能专长
        </div>
        <div class="nberN07">
        {/foreach $resume_skills as $key=>$value/}
        	<p class="nberTit2 nberTit2x">
        	<b>{/$value['skill_name']/}</b><span>{/get_name skill_level=$value['skill_level']/}</span>
        	</p>        	
        {//foreach/}
        </div>
        {//if/}
        {/if count($resume_certificates)>0/}
        <div class="sumeNtit">
        	<em class="titIcon08"></em>
            证书
        </div>
        <div class="nberN07">
        {/foreach $resume_certificates as $key=>$value/}
        	<p class="nberTit2 nberTit2x">
        	<b>{/$value['certificate_name']/}</b><span>{/$value['gain_time']/}获得</span>
        	</p>      	
        {//foreach/}
        </div>
        {//if/}
        {/if count($resume_appends)>0/}
        <div class="sumeNtit">
