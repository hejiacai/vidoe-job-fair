<script>

var selece_myjob = $('input[name=myjob]');
selece_myjob.click(function(){
    var url = '{/$siteurl.company|replace:"http:":""/}/index/JobList/status-0-position-joblist?';
    if(selece_myjob.is(':checked')){
        url += '&ismyjob=true';
    }
    window.location.href = url;
});

var joblist = {
	initialize:function(){
		$('#upAllSelect').bindCheckBox('chkjob', '#lstBox');
		$('#downAllSelect').bindCheckBox('chkjob', '#lstBox');

        $('#upAllSelect').click(function() {
			if ($(this).is(':checked')) {
				$('#downAllSelect').attr('checked', 'checked');
			} else {
				$('#downAllSelect').removeAttr('checked');
			}
		});

        $('#downAllSelect').click(function() {
			if ($(this).is(':checked')) {
				$('#upAllSelect').attr('checked', 'checked');
			} else {
				$('#upAllSelect').removeAttr('checked');
			}
		});

		$('.reason').hover(function(){
			$(this).find('.show_alt').toggle();
		});
//		$('.reason').mouseleave(function(){
//            $('.show_alt').hide();
//		});

		$('#lst2').find('tr').hover(function(){
			$(this).addClass('hov');
		},function(){
			$(this).removeClass('hov');
		});

		$('.watermark').watermark2();
	},
	deleteJob : function(job_id) {
		$.confirm('删除后将不可恢复，确定要删除吗？', '操作提示', function() {
			$.getJSON('{/get_url rule="/index/delete/"/}', {job_id:job_id}, function (json) {
	            if (json.error) {
	                $.message(json.error, {title : '系统提示', icon : 'fail'});
	                return;
	            }

	            if (json.success) {
	                var jobListStopCount = parseInt($("#jobListStopCount").html());

                    $("#jobListStopCount").html(jobListStopCount - 1);
	                $("#li" + job_id).remove();

	                $.anchorMsg('删除成功');

	                if ($('#lstBox').find('tr').length <= 0) {
		        		$('#lst2').remove();
		        		$('div.noData').show();
		        	}
	            }
	        });
		});
	},
	deleteChooseJobs : function() {
		var checkboxs = $('#lstBox').find('input[name="chkjob"]:checked'),
			jobids = [];
   	   	for (var i = 0, len = checkboxs.length; i < len; i += 1) {
   	   		jobids.push($(checkboxs[i]).val());
   	   	}

   	 	if (jobids.length <= 0) {
			$.anchor('请选择需要删除的职位', {icon:'info'});
			return;
		}

   	 	$.confirm('删除职位后不能恢复，确定删除吗？', '操作提示', function() {
			$.getJSON('{/get_url rule="/index/deletechoosejob/" data="jobids=' + jobids.join('-jobids-') + '&v='+Math.random()+'"/}', function (json) {
	            if (json.error) {
	                $.message(json.error, {title: '系统提示', icon: 'fail'});
	                return;
	            }

	            if (json.fail) {
	            	$.message(json.failitem, {title: '系统提示', icon: 'fail'});
	            } else if(json.success) {
	            	$.anchorMsg('删除职位成功');
	            }

	            if (json.job_ids) {
		            var count = json.job_ids.length;
		            if (count > 0) {
		                var jobListStopCount = parseInt($("#jobListStopCount").html());

		                $("#jobListStopCount").html(jobListStopCount - count);
		                $.each(json.job_ids, function (i, item) {
	            			$("#li" + item).remove();
	            		});

		                if ($('#lstBox').find('tr').length <= 0) {
			        		$('#lst2').remove();
			        		$('div.noData').show();
			        	}
		            }
	            }
	        });
		});
	}
};

joblist.initialize();

function banner_on() {
    joblist.tanchuang();
}

function clocse_show_banner() {
    $("#show_banner").hide();
}


$.getJSON('{/get_url rule="/account/twodimensioncode/"/}',function(result){
    if(result&&result.status) {
        var src = result.data.codeurl;
        $('#ewmBox img').attr('src',src);
        $('#ewmBox').show();
    }
});

$("#ewmBox,#ewmBox1").find("a").click(function(){$(this).parents(".ewmBox").hide();return false;});
</script>

