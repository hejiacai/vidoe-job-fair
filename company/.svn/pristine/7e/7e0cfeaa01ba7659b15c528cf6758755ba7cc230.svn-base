                                    		<a title="{/$job_list[job].station/}" class="{/if $job_list[job].check_state == 4/}aud{/else/}jobName{//if/}" {/if $status==$stop_job_status && $job_list[job].check_state!=2 && $job_list[job].check_state!=4/}href="{/get_url rule="/job/mod/" data="job_id={/$job_list[job].job_id/}"/}" target="_blank"{/else/}href="javascript:void(0);"{//if/}>{/$job_list[job].sub_station/}</a>
	                                    	{/if $status == $stop_job_status && $job_list[job].check_state == 2/}
	                                		<b>审核未通过</b>
                                            {/if $job_list[job].audit_remark/}
                                                <div class="reason" >
                                            {/base_lib_BaseUtils::cutstr($job_list[job].audit_remark,45,"utf-8","","...")/}
                                                    {/*{/$job_list[job].audit_remark/}*/}
                                                    <div class="show_alt" style="{/if mb_strlen($job_list[job].audit_remark)>20/}width:450px{//if/}">
                                                        {/html_encode content=$job_list[job].audit_remark/}
                                                        {/if $job_list[job]['error_msg_button']/}
                                                            <br/>
                                                            {/$job_list[job]['error_msg_button']/}
                                                        {//if/}
                                                    </div>
                                                </div>
                                                {//if/}
	                                		{/elseif $status == $stop_job_status && $job_list[job].check_state == 4/}
	                                		<b title="我们会在1个工作日内完成审核，在此期间求职者将无法看到该职位">审核中</b>
                                            {/elseif $status == $stop_job_status && $job_list[job].check_state == 9/}
                                            <b title="">失效</b>
	                                		{/else/}
	                                		{/if !base_lib_BaseUtils::nullOrEmpty($job_list[job].urgent_mark) && $job_list[job].urgent_mark == true/}
                                            <em class="num" title="{/$job_list[job].urgent_end_date/}截止">急</em>
                                            {//if/}
	                                		{//if/}
                                		</p>
                                        {/if $job_list[job]['generation_bidding']/}
                                            <p style="color:#999">
                                                {/base_lib_BaseUtils::cutStr($accounts[$job_list[job]['company_id']]['company_name_display'], 15, 'utf-8', '', '...')/}
                                                <b class="gen-binding">代招</b>
                                            </p>
                                        {//if/}
                                   	</td>
                                    <!--发布人-->
                                    <td>{/$job_list[job].account_user_name/}</td>
                                    <td class="wid120">
                                    	{/if $status==$stop_job_status && $job_list[job].check_state==2/}<em>_</em>{/else/}<a {/if $job_list[job].applyCount>0/}target='_blank' href='{/get_url rule="/apply/Index/" data="job_id={/$job_list[job].job_id/}&show_model=1&search_model=0"/}'{/else/}href='javascript:void(0);'{//if/}>{/$job_list[job].applyCount/}</a>{//if/}
                                    </td>
                                    <td class="wid120">
                                        {/if $status == $stop_job_status && $job_list[job].check_state == 2/}
                                        <em>_</em>
                                        {/else/}
                                        <span>
                                            <a {/if $job_list[job].applyNotReadCount > 0/}target='_blank' href='{/get_url rule="/apply/Index/" data="job_id={/$job_list[job].job_id/}&child_status=2&show_not_use=0"/}'{/else/}href='javascript:void(0);'{//if/}>{/$job_list[job].applyNotReadCount/}</a>
                                            {/if $job_list[job].applyNotReadCount > 0/}
                                            <img src="{/$siteurl.style/}/img/c/new_index/reddian.png" />
                                            {//if/}
                                        </span>
                                        {//if/}
                                    </td>
                                    <td class="wid120">
                                        {/if $status==$stop_job_status && $job_list[job].check_state==2/}<em>_</em>{/else/}
                                        <span>
                                            <a {/if $job_list[job].no_reply_num > 0/}target='_blank' href='{/get_url rule="/apply/Index/" data="job_id={/$job_list[job].job_id/}&show_not_use=0"/}'{/else/}href='javascript:void(0);'{//if/}>{/$job_list[job].no_reply_num/}</a>
                                            {/if $job_list[job].no_reply_num > 0/}
                                            <img src="{/$siteurl.style/}/img/c/new_index/reddian.png">
                                            {//if/}
                                        </span>
                                        {//if/}
                                    </td>
                                    <td class="wid120">
                                    	{/if $status == $stop_job_status && ($job_list[job].check_state == 2 || $job_list[job].check_state == 9)/}
                                		<em>{/date('Y-m-d', strtotime($job_list[job].end_time))/}</em>
                                		{/else/}
                                        {/if $status == $stop_job_status && $job_list[job].status == 0/}
                        				{/if strtotime(date('Y-m-d',strtotime($job_list[job].end_time)))-strtotime(date('Y-m-d',time()))>=0/}
                        				<em>{/date('Y-m-d', strtotime($job_list[job].end_time))/}</em>
                       					{/else/}
                       					<em>已过期</em>
                        				{//if/}
                               			{/else/}
                                        {/if $job_list[job].hidden_end_time == false/}<em class="red">{/$job_list[job].end_time_name/}</em>{/else/}<em>已过期</em>{//if/}</em>
                                		{//if/}
                                		{//if/}
                                    </td>
                                    <td class="wid140">
                                        <span class="tipTxt">
                                        {/if $status==$stop_job_status/}
                            			<a href="{/get_url rule="/job/mod/" data="job_id={/$job_list[job].job_id/}"/}" class="editLnk" target="_blank">重新发布</a>
                            			{//if/}
                                        <a onclick="joblist.deleteJob({/$job_list[job].job_id/})" href="javascript:void(0);" class="lnk">删除</a>
                                        </span>
                                    </td>
                                </tr>
                                {//section/}
                            </tbody>
                        </table>
                        <div class="allBtn">
                        <span class="formChb"><input id="downAllSelect" type="checkbox" class="chb"><label for="downAllSelect">全选</label></span>
                        <a href="javascript:void(0);" onclick="joblist.deleteChooseJobs();" class="btn3 btnsF12">删除</a>
                    </div>
                    </div>
                    <div class="noData" {/if !empty($job_list) && count($job_list) > 0/}style="display:none;"{//if/}>
                    	<p>{/if !base_lib_BaseUtils::nullOrEmpty($search_keyword)/}未找到相关职位，请更换关键词重试或<a href='{/get_url rule="/index/joblist/" data="status=$stop_job_status"/}'>查看全部职位</a>{/else/}没有停止招聘的职位{//if/}</p>
                    </div>
                </div>
            </div>
        </hgroup>
    </section>

    <div class="clear"></div>
</div>
