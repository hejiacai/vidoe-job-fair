<body id="body">
{/include 'new_header.html' par="全职招聘" cur='数据报表'/}


<div class="resumentNbg resumentNbg-new">
	{/if $left_type==2/}
	{/include 'resume/apply/nav.html' cur="offer管理"/}
	{/else/}
	<div class="rMentLt">
		<dl class="rMentDl">
			<dd>
				<a class="md_job_zd" href="{/get_url rule='/readjob/CompanyVisit'/}" >
					<i class="icon-menu-work"></i><span>职位诊断</span>
				</a>
			</dd>
			<dd>
				<a class="md_recruit_effect" href="{/get_url rule='/readjob/recruitmenteffect'/}">
					<i class="icon-svg28"></i><span>招聘效果报表</span>
				</a>
			</dd>
			<dd class="cut">
				<a class="md_recruit_effect" href="{/get_url rule='/readjob/GetResemeRecruitmentEffect'/}">
					<i class="icon-svg40"></i><span>获取简历报表</span>
				</a>
			</dd>
		</dl>
	</div>
	{//if/}

	<!-- 右侧内容 -->
	<div class="rMentRt" style="background: #fff;">
		<div class="content">
			<hgroup>
				<div class="part part1">
					<form id="frmInvite" method="get" action="{/get_url rule='/readjob/RecruitmentEffect/'/}" class="offerSearch">
						<input type="hidden" name="left_type" value="{/$left_type/}">
						<!-- 筛选条件 -->
						<div class="filterOptions">
							<div class="subMetx colBox">
							    <span style="padding-top:3px;">获取日期：</span>
							    <input class="Wdate text dateContainerStyle" type="text" onClick="WdatePicker({onpicked:function(){$('.j_time_search_btn').click();},oncleared:function(){$('.j_time_search_btn').click();}})" id="start_time" name="start_time" readonly value="{/$s_time/}" />
							    <b> ~ </b>
							    <input class="Wdate text dateContainerStyle" type="text" onClick="WdatePicker({onpicked:function(){$('.j_time_search_btn').click();},oncleared:function(){$('.j_time_search_btn').click();}})" id="end_time" name="end_time" readonly value="{/$e_time/}" />
							    <div class="clear"></div>
							</div>
							<div class="subMetx colBox">
								<span>招聘负责人：</span>
								<div class="xkTabSelect inB"><span id="recruitmentManager" class="drop zindex" style="z-index: 5;"></span></div>
							</div>
							<div class="clearfix" style="padding-top: 18px;">
							{/if $is_show_company_search/}
							<div class="subMetx  colBox">
								<span>公司：</span>
								<div class="xkTabSelect inB"><span id="jobCompany" class="drop zindex"></span></div>
							</div>
							{//if/}

							<span>公司：</span>
							<div class="xkTabSelect inB"><span id="tstDropJob" class="drop zindex"></span></div>
							
							<!-- <label style="padding-top:6px; margin-right: 15px;">
								<input class="md_stopjob"  type="checkbox" value="1" id="showStopJob" style="display:inline-block; vertical-align:-1px;" {/if !empty($show_not_use_job)/}checked="checked"{//if/}>
								&nbsp;包含停招职位
							</label> -->

							<!-- 搜索按钮 -->
						</div>
							<div class="searchContainer">
								<input type="button" value="搜索" id="go_search" class="mianshiSearchBtn md_recruit_select">
								<input type="button" value="导出Excel" id="exel_smit" class="mianshiSearchBtn md_recruit_excel">
							</div>
							<input type="hidden" name="show_not_use" value="{/if !empty($show_not_use_job)/}1{/else/}0{//if/}">
							<input type="hidden" name="execl" value="0">
							<input type="hidden" name="page" value="{/$cur_page/}">
							</div>
					</form>

					<div class="offerTipsx">
						<img src="{/$siteurl.style/}/img/company/video/eng_icon09.png" >
						<span>需及时设置面试结果和入职结果，以免面试和入职数据不准确</span>
						<a class="md_recruit_set" href="/invitev1/index/-audition_result-0-left_type-1" target="_blank">去设置</a>
					</div>

					<div class="offerDatas">
						<table class="offerTable">
							<thead class="offerTbHead">
								<tr>
								<!--<th width="200" class="name">招聘职位</th>
									<th width="150">收到简历</th>
									<th width="158">邀请面试	</th>
									<th width="162">面试到场</th>
									<th width="163">面试通过</th>
									<th width="150">入职人数</th> -->
									<th width="200" class="name">获取时间</th>
									<th width="150">获取简历</th>
									<th width="158">邀请面试	</th>
									<th width="158">面试爽约	</th>
									<th width="162">面试未通过</th>
									<th width="163">面试通过</th>
									<th width="150">入职人数</th>
								</tr>
							</thead>
							<tbody>
								<tr>
<!-- 								<td class="name overtext" tips="{/$v['station']/}">{/$v['station_new']/}</td>
									<td class="overtext pos">{/$v['total_count']/}</td>
									<td class="overtext"><a href="javascript:viod(0);">{/$v['invit_num']/}</a></td>
									<th width="158"><a href="javascript:viod(0);">面试爽约</a></th>
									<td class="overtext"><a href="javascript:viod(0);">{/$v['present_num']/}</a></td>
									<td class="overtext"><a href="javascript:viod(0);">{/$v['pass_num']/}</a></td>
									<td class="overtext"><a href="javascript:viod(0);">{/$v['entry_num']/}</a></td> -->
									
									<td class="name overtext" tips="{/$v['station']/}">88</td>
									<td class="overtext pos"><a href="javascript:viod(0);">33</a></td>
									<td class="overtext"><a href="javascript:viod(0);">77</a></td>
									<th width="158"><a href="javascript:viod(0);">6</a></th>
									<td class="overtext"><a href="javascript:viod(0);">55</a></td>
									<td class="overtext"><a href="javascript:viod(0);">66</a></td>
									<td class="overtext"><a href="javascript:viod(0);">8</a></td>
								</tr>
							</tbody>
						</table>
						<div class="noData">
							<!-- <p>未找到数据，请更换筛选条件再试！</p> -->
	                        <!-- <a class="btn" href="javascript:void(0);" id="btnClearFilterSearch"><i class="hbFntWes">&#xf014;</i>清空筛选/搜索条件</a></p> -->
						</div>

					</div>
				</div>
			</hgroup>
			{/$page/}
		</div>
	</div>
<div class="j_time_search_btn" data-explain="do not delete!" style="display: none"></div>
<div class="j_username_search_btn" data-explain="do not delete!" style="display: none"></div>
<script  type="text/javascript">
    var action_url = '{/$siteurl.style/}';
    action_dom = [
        ['.md_job_zd', 420],
        ['.md_recruit_effect', 421],
        ['.md_recruit_select', 422],
        ['.md_recruit_set', 423],
        ['.md_recruit_excel', 424],
    ];
</script>
<script type="text/javascript" language="javascript" src="{/version file='action.js'/}"></script>

	<script type="text/javascript">
    //时间    
		$('#timeYear').droplist({
		    defaultTitle : '2019年',
		    style : 'width:105px;',
		    noSelectClass : '#444',
		    inputWidth : 105,
		    width : 128,
		    hddName : 'year',
		    items :  [],
			selectValue : '{/$year/}',
		    maxScroll : 10,
		    onSelect : function(i, name) {
		    	//选中后的事件
		    	$('#frmInvite').submit()
			}
		});
		
		$('#timeMonth').droplist({
		    defaultTitle : '10月',
		    style : 'width:105px;',
		    noSelectClass : '#444',
		    inputWidth : 105,
		    width : 128,
		    hddName : 'month',
		    items : [],
			selectValue : '{/$month/}',
		    maxScroll : 10,
		    onSelect : function(i, name) {
		    	//选中后的事件
		    	$('#frmInvite').submit()
			}
		});
	//招聘负责人recruitmentManager
		$('#recruitmentManager').droplist({
		    defaultTitle : '',
		    style : 'width:105px;',
		    noSelectClass : '#444',
		    inputWidth : 105,
		    width : 128,
		    hddName : 'account_id',
		    items : [],
			selectValue : '{/$son_account_id/}',
		    maxScroll : 10,
		    onSelect : function(i, name) {
		    	//选中后的事件
        		$("input[name=job_id]").val('');//清空职位
		    	$('#frmInvite').submit()
			}
		});
	
	//职位	
	    $('#tstDropJob').droplist({
	        defaultTitle : '全部职位',
	        style : 'width:150px;',
	        noSelectClass : '#444',
	        inputWidth : 150,
	        width : 173,
	        hddName : 'job_id',
	        items : [],
	    	selectValue : '{/$job_id/}',
	        maxScroll : 10,
	        onSelect : function(i, name) {
	        	//选中后的事件
	        	$('#frmInvite').submit()
	    	}
	    });
	//公司
		$('#jobCompany').droplist({
		    defaultTitle : '',
		    style : 'width:105px;',
		    noSelectClass : '#444',
		    inputWidth : 105,
		    width : 128,
		    hddName : 'company_id',
		    items : [],
			selectValue : '{/$company_id/}',
		    maxScroll : 10,
		    onSelect : function(i, name) {
		    	//选中后的事件
        		$("input[name=job_id]").val('');//清空职位
		    	$('#frmInvite').submit()
			}
		});	
		
	$("#showStopJob").change(function(){
        if (!$("#showStopJob").attr("checked")) {
            //不包含停招职位
            isShowStopJob = 2;
            $("input[name=show_not_use]").val(0);
        }else{
            $("input[name=show_not_use]").val(1);

		}
        $("input[name=job_id]").val('');//清空职位
	    $('#frmInvite').submit()
	})



	$('#exel_smit').click(function(){
		$("input[name=execl]").val(1);
		$('#frmInvite').submit();
        $("input[name=execl]").val(0);
	})
    $('#go_search').click(function(){
        $("input[name=execl]").val(0);
        $('#frmInvite').submit();
    })



	</script>
</div>

{/include file="msgpop.html"/}
{/include file="footer.html"/}
{/include file="chat/chat.html"/}
</body>
</html>
