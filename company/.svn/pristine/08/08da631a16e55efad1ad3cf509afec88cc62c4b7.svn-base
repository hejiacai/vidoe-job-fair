#fancybox-thumbs ul li img {display: block;position: relative;border: 0;padding: 0;max-width: none;}

</style>
<script type="text/javascript">
var isDownLoad     = false,
	resume_id      = "{/$resume_id/}",
	apply_id       = "{/$applyid/}",
	company_id     = "{/$company_id/}",
	src            = "{/$src/}",
	recommendid    = '{/$recommendid/}';
       
hbjs.use('tools.cookie, widge.overlay.hbDialog, widge.overlay.confirmBox, product.hbCommon', function(cookie, Dialog, confirmBox, hbCommon, $, util) {
	var isIE = !window.XMLHttpRequest,
		isIE6 = !-[1,] && isIE;
	$ = $.extend(hbCommon);
	
	var historyContainer 	 = $('#historyContainer'),
		historyList          = historyContainer.children('ul'),
		historyItem          = historyList.children('li'),
		historyMore          = historyItem.last(),
		historyShowMsg       = '隐藏历史记录<i class="hbFntWes">&#xf106;</i>',
		historyHidMsg        = '显示历史记录<i class="hbFntWes">&#xf107;</i>';
	
	$('#historyContainer').on('click', '#btnHistory, #btnMoreHistory', function (e) {
		var target = $(e.currentTarget),
			idName = target.attr('id');

		if (idName === "btnHistory") {
			if (target.attr('data-status') == 1) {
				historyItem.hide();
				target.attr('data-status', 0).html(historyHidMsg);
			} else {
				historyList.children('.history30').show();
				if (historyMore.length) {
					historyMore.show();
				}
				target.attr('data-status', 1).html(historyShowMsg);
			}
		} else {
			historyItem.show();
			historyMore.hide();
		}
	});
		
	{/if !empty($avatar_big)/}
	$('#avatarImgBox').on('mouseover', function (e) {
		$.showPhotoHD(this, null, '{/$avatar_big/}');
	});
	{//if/}
		
	searchAttribution('{/$mobile_phone/}');
	//保存到电脑
	var dialog = new Dialog({
		idName: 'save-comp',
		title: '保存到电脑',
		close: 'x',
		isAjax: true
	}),
	eventName = [
		'#btnSaveComputer',
		'#btnNetWorkSuccessInvite',
		'#btnNetWorkInvite',
		'#btnFairInvite',
		'#btnJeject',
		'#btnGetLinkway2',
		'#btnRemark',
		'#btnFav',
		'#relLnk',
		'#btnPrint',
		'#btnDelete',
		'#btnReport'
	],
	saveComputerHTML = [
		'<div class="save-comp-box"><span class="gray9">文件另存为：</span>',
		'<ul class="clearfix"><li class="doc">',
		'<a href="{/get_url rule="/resume/worddown/"/}resumeid-' + resume_id + '-src-'+src+'-applyid-'+apply_id+'">.doc</a>',
		'</li><li class="html">',
		'<a href="{/get_url rule="/resume/htmldown/"/}resumeid-' + resume_id + '-src-'+src+'-applyid-'+apply_id+'">.html</a>',
		'</li><li class="pdf">',
		'<a href="{/get_url rule="/resume/pdfdown/"/}resumeid-' + resume_id + '-src-'+src+'-applyid-'+apply_id+'">.pdf</a>',
		'</li></ul></div>'
	].join(''),
	linkwayHTML = [
		'<p class="link">',
		'<span {phoneDisplay}><i class="{phoneStatus}"></i><strong>{phone}</strong><em id="phoneAddr"></em></span>',
		'<span {mailDisplay}><i class="{mailStatus}"></i><strong>{mail}</strong></span>',
		'<span {qqDisaplay} class="last"><i class="{qqStatus}"></i><strong>{qq}</strong></span></p>'
	].join('');

	var pWidth = 70,
		fontWidth = 18;
	
	function getDomID(name) {
		return name.replace('#', '');
	}

	function showModel(icon, msg) {
		confirmBox.timeBomb(msg, {
			name: icon,
			width: pWidth + msg.length * fontWidth
		});
	}

	function linkwayCallback(json) {
		var linkwayContainer = $('#linkwayContainer'),
			nolink           = linkwayContainer.children('#linkWayBar'),
			dataObj          = {};
		
		if (nolink.length) {
			if (json.mobile_phone_is_validation == 1) {
				dataObj['phoneStatus'] = "p";
			} else {
				dataObj['phoneStatus'] = "nop";
			}

			dataObj['phone']        = json.mobile_phone || '';
			dataObj['phoneDisplay'] = dataObj['phone'] ? '' : 'style="display:none"';
			searchAttribution(dataObj['phone']);
			
			if (json.email_is_validation == 1) {
				dataObj['mailStatus'] = 'm';
			} else {
				dataObj['mailStatus'] = 'nom';
			}

			dataObj['mail']        = json.email || '';
			dataObj['mailDisplay'] = dataObj['mail'] ? '' : 'style="display:none"';			
			dataObj['qq']          = json.qq || '';
			dataObj['qqDisplay']   = dataObj['qq'] ? '' : 'style="display:none"';
			dataObj['qqStatus']    = dataObj['qq'] ? 'q' : 'noq';
			
			linkwayContainer.html(util.string.replace(linkwayHTML, dataObj));
			$('#btnGetLinkway2').hide();
			if (src == 'recommend') {
				// 更新推荐简历的下载状态
				$.getJSON('{/get_url rule="/recommend/setstatus/"/}', {recommendid:recommendid}, function(result) {
				});
			}

			$('#reportLinkway').show();
			$('#btnReport').parent().show();
		}
	}

	function searchAttribution(tel) {
		//获取号码归属地
		var regex 	  = /^1[3|4|5|8][0-9]\d{4,8}$/,
			phoneAddr = $('#phoneAddr');

		if (!phoneAddr.length) return;
		if (tel && !regex.test(tel)) {
			phoneAddr.html('（未知）');
		} else {
			var url = 'http://tcc.taobao.com/cc/json/mobile_tel_segment.htm?tel=' + (tel || '');
			if (isIE6) {
				url = 'https://www.baifubao.com/callback?cmd=1059&callback=phone&phone=' + (tel || '');
			}

			$.ajax({
				async : false,
				type : "post",
				url : url,
				dataType : "jsonp",
				contentType : "application/x-www-form-urlencoded; charset =utf-8",
				jsonp : "callback",
	            success : function(json) {
	                if (isIE6) {
						if (json.data.area == '' || json.data.area == undefined) {
							phoneAddr.html('（未知）');
						} else {
							phoneAddr.html('（' + json.data.area + '）');
						}
	                } else {
	                	if (json.province == ''|| json.province == undefined) {
	    			  		phoneAddr.html('（未知）');
	    				} else {
	    					phoneAddr.html('（'+json.province+'）');
	    				}
	                } 
				},
				error : function () {   
					phoneAddr.html('（未知）');
				}
			});
		}
	}

	function openRemark(idName) {
		if (resume_id != undefined && resume_id > 0) {
			var lastName = idName === getDomID(eventName[6]) ? ('-v-' + Math.random()) : '';
			
			dialog.resetSize('auto').setContent({
				title:'备注',
				content:'{/get_url rule="/resumeremark/index/"/}resume_id-' + resume_id + lastName
			}).off('loadComplete').on('loadComplete',function () {
				this.oneCloseEvent('#btnCancel');
			}).show();
		
		} else {
			showModel('fail', '请选择一个简历后再进行备注');
		}
	}

	function getLinkway() {
		$.getJSON('{/get_url rule="/download/checkbalance"/}', {
			resume_id : resume_id
		}, function (json) {
			if (json && !json.status) {
				dialog.resetSize(510).setContent({
	            	title : '查看联系方式',
	            	content : '<dl id="yaoqing-alert" class="yaoqing-alert clearfix">'
							+ '<dt><img src="{/$siteurl.style/}/img/company/cjl.jpg" /></dt><dd>'
							+ '<p class="t1">哎呀，余额不足了！</p>'
							+ '<p class="t2">当前帐号余额：<span>￥'  + json.account_overage + '</span><br />简历需花费：'
							+ '<span>￥{/number_format($resumelevel["consume"], 2)/}</span></p>'
							+ '<p><a href="{/get_url rule="/pay/"/}" target="_blank" class="btn1 btnsF14 charge">立即充值</a><a href="{/get_url rule="/index/memberdetail/"/}" class="link2">成为会员，下载简历低至2元</a></p>'
							+ '</dd></dl>'
	            }).show();
			} else {
				var cookieValue = cookie.get('downresumeprompt');
				if (cookieValue && cookieValue.length > 0) {
					$.getJSON('{/get_url rule="/download/getlinkway"/}',{
						resumeID : resume_id, 
						companyID : company_id
					}, function(json) {
						resultLinkWay(json);
					});
				} else {
					dialog.resetSize('auto').setContent({
						title: '获取联系方式',
						content: '{/get_url rule="/download/deductcountprompt" data="resumeid=$resume_id"/}'
					}).off('loadComplete').on('loadComplete',function(){
						this.oneCloseEvent('#btnDeduct', function(){
							if (isDownLoad) {
		                        document.getElementById("have_download_resume").value = "1";
								$.getJSON('{/get_url rule="/download/getlinkway"/}',{
									resumeID : {/$resume_id/},
									companyID : {/$company_id/}
								}, function(json){
									resultLinkWay(json);
								});
							}
						});
					}).show();
				}
			}
		});
	}
	
	$(".save-comp").on("click", ".charge", function() {
		dialog.hide();
	})

	function resultLinkWay(json) {
		if (json && json.error) {
			showModel('fail', json.error);
			return ;
		}

		linkwayCallback(json);
		searchAttribution(json.mobile_phone);
		
		dialog.resetSize('auto').setContent({
			title:'标记为感兴趣',
			content: '{/get_url rule="/mark/markdown-resumeid-{/$resume_id/}-operate-down.html"/}'
		}).off('loadComplete').on('loadComplete',function() {
			this.oneCloseEvent('#btnSava');
		}).show();
	}

	$('#btnReportClose').on('click',function(){
		$('#reportLinkway').hide();
	});
	$('#btnGetLinkway').on('click', function(){
		getLinkway();
	});
        
    $("#go_on_vip").on('click',function(){
        dialog.resetSize('auto').setContent({
			title: '续费',
			content: '{/get_url rule="/account/showApplyVipTipsV2/"/}'
		}).show();
    });
	
	$("#open_vip").on('click',function(){
		dialog.resetSize('auto').setContent({
			title: '开通会员',
			content: '{/get_url rule="/account/showApplyVipTipsV2/"/}'
		}).show();
	                   
	});
    
    $("body").on("click", "#closeClick", function() {
        dialog.hide();
    });

	$('#btnReport2').on('click',function(){
		report2();
	});

    function report2(){
        var have_download_resume = '0';
        if ($("#have_download_resume").val() == '1'){
            have_download_resume = '1';//have_download_resume
        }
		$.getJSON('{/get_url rule="/report/CheckReported/" data="resume_id={/$resume_id/}&person_id={/$person_id/}"/}'+'-v-' + Math.random(), function(result){
			if(result.success) {
				dialog.resetSize(460).setContent({
					isOverflow : true,
					title : '举报',
                    content : '{/get_url rule="/report/index/" data="resume_id={/$resume_id/}&person_id={/$person_id/}&phone={/$mobile_phone/}&resumesrc={/$src/}&downloaded='+have_download_resume+'"/}'
				}).off('loadComplete').on('loadComplete',function(){
					this.oneCloseEvent('#btnReportSubmit');
				}).show();
			} else {
				showModel('info', result.error);
			}
		});
    }

	$(".save-comp").on("click", '.close', function() {
		dialog.hide();
	});
    
	$('#right-operat').on('click', eventName.join(','), function(e) {
		var target = $(e.currentTarget),
			idName = target.attr('id');
			
		if(!cookie.get('userid') || cookie.get('usertype') != 'c'){
			window.location.href = '{/get_url rule="/login"/}';
			return;
		}
			
		switch(idName){
			case getDomID(eventName[0]):
				dialog.resetSize(500).setContent(saveComputerHTML).show();
				break;

			/* 同意面试弹窗 */
			case getDomID(eventName[1]):


				$.getJSON('{/get_url rule="/download/checkbalance"/}', {
					resume_id : resume_id
				}, function (json) {
					if (json && !json.status) {
						dialog.resetSize(510).setContent({
			            	title : '查看联系方式',
			            	content : '<dl id="yaoqing-alert" class="yaoqing-alert clearfix">'
									+ '<dt><img src="{/$siteurl.style/}/img/company/cjl.jpg" /></dt><dd>'
									+ '<p class="t1">哎呀，余额不足了！</p>'
									+ '<p class="t2">当前帐号余额：<span>￥'  + json.account_overage + '</span><br />简历需花费：'
									+ '<span>￥{/number_format($resumelevel["consume"], 2)/}</span></p>'
									+ '<p><a href="{/get_url rule="/pay/"/}" target="_blank" class="btn1 btnsF14 close">立即充值</a><a href="{/get_url rule="/index/memberdetail/"/}" class="link2">成为会员，下载简历低至2元</a></p>'
									+ '</dd></dl>'
			            }).show();
					} else {
						dialog.resetSize('auto').setContent({
							title: '同意面试',
							content: '{/get_url rule="/invite/invitesingleshow/" data="applyID='+ apply_id + '"/}-v-' + Math.random()
						}).off('loadComplete').on('loadComplete', function() {
							this.oneCloseEvent('#btnSendInvite');
						}).show();
					}
				});
				break;

			/* 发送面试邀请 */
			case getDomID(eventName[2]):
				$.getJSON('{/get_url rule="/download/checkbalance"/}', {
					resume_id : resume_id
				}, function (json) {
					if (json && !json.status) {
						dialog.resetSize(510).setContent({
			            	title : '查看联系方式',
			            	content : '<dl id="yaoqing-alert" class="yaoqing-alert clearfix">'
									+ '<dt><img src="{/$siteurl.style/}/img/company/cjl.jpg" /></dt><dd>'
									+ '<p class="t1">哎呀，余额不足了！</p>'
									+ '<p class="t2">当前帐号余额：<span>￥'  + json.account_overage + '</span><br />简历需花费：'
									+ '<span>￥{/number_format($resumelevel["consume"], 2)/}</span></p>'
									+ '<p><a href="{/get_url rule="/pay/"/}" target="_blank" class="btn1 btnsF14">立即充值</a><a href="{/get_url rule="/index/memberdetail/"/}" class="link2">成为会员，下载简历低至2元</a></p>'
									+ '</dd></dl>'
			            }).show();
					} else {
						dialog.resetSize('auto').setContent({
							isOverflow : true,
							title : '邀请简历',
							content : '{/get_url rule="/invite/invitesingleshow/" data="resumeID=' + resume_id + '"/}'
						}).off('loadComplete').on('loadComplete', function() {
							this.oneCloseEvent('#btnSendInvite');
						}).show();
					}
				});
				break;

			/* 打开备注 */
			case getDomID(eventName[3]):
				openRemark(idName);
				break;

			/* 拒绝面试 */
			case getDomID(eventName[4]):
				// 婉拒求职者
				$.getJSON('{/get_url rule="/apply/refuse/"/}'+'op-refuse-ids-' + apply_id + '-v-' + Math.random(), function(result){
					if(result.success) {
						showModel('success', '已成功标记为不合适');
					} else {
						showModel('fail', result.error);
					}
	   			});
				break;

			/* 下载简历联系方式 */
			case getDomID(eventName[5]):
				getLinkway();
				break;

			/* 打开备注 */
			case getDomID(eventName[6]):
				openRemark(idName);
				break;
			
			/* 标记为感兴趣的简历 */
			case getDomID(eventName[7]):
				$.getJSON('{/get_url rule="/fav/fav"/}', {
					resume_id : {/$resume_id/},
					company_id : {/$resume_id/},
					r : Math.random().toString().replace('.', '0')
				}, function (json) {
					if (json && json.error) {
						showModel('fail', json.error);
						return;
					}

					if (json.fav_type == "add") {
						dialog.resetSize('auto').setContent({
							title : '标记为感兴趣',
							content : '{/get_url rule="/mark/markfav-resumeid-{/$resume_id/}-operate-fav.html"/}'
						}).off('loadComplete').on('loadComplete', function () {
							this.oneCloseEvent('#btnFavSava');
							target.parent().addClass('cur');
						}).show();
						return;
					}

					if (json.fav_type == "cancel") {
						showModel('success', '取消感兴趣成功');
						target.parent().removeClass('cur');
						return;
					}

				});
				break;

			/* 转发到邮箱 */
			case getDomID(eventName[8]):
				dialog.resetSize('auto').setContent({
					title : '转发到邮箱',
					content : '{/get_url rule="/resume/wordsend/" data="resumeid='+resume_id+'&src='+src+'&applyid='+apply_id+'"/}'
				}).off('loadComplete').on('loadComplete', function () {
					this.oneCloseEvent('#btnSendWordToEmail');
				}).show();
				break;

			/* 打印简历*/
			case getDomID(eventName[9]):
				var url = '{/get_url rule="/resume/htmlprint" data="resumeid='+resume_id+'&src='+src+'&applyid='+apply_id+'"/}';         
				if (isIE) {
					target.attr('href', url).attr('target', '_blank');
				} else {
					$('#printIframe').attr("src", url);
				}
				break;

			/* 放入回收站 */
			case getDomID(eventName[10]):
				confirmBox.confirm('您确定将简历放入到回收站', null, function() {
					var self = this;
					$.getJSON('{/get_url rule="/apply/DeleteApply/"/}'+'op-del-ids-' + apply_id + '-v-'+Math.random(),function(result){
						if (result.success) {
							showModel('success', '已放入回收站');
						} else {
							showModel('fail', result.error);
						}
						self.hide();
					});
				});
				break;

			/* 举报简历 */
			case getDomID(eventName[11]):
                var have_download_resume = '0';
                if ($("#have_download_resume").val() == '1') {
                    have_download_resume = '1';
                }

				$.getJSON('{/get_url rule="/report/CheckReported/" data="resume_id={/$resume_id/}&person_id={/$person_id/}"/}'+'-v-' + Math.random(), function (result) {
					if (result.success) {
						dialog.resetSize(460).setContent({
							isOverflow: true,
							title: '举报',
							content: '{/get_url rule="/report/index/" data="resume_id={/$resume_id/}&person_id={/$person_id/}&phone={/$mobile_phone/}&resumesrc={/$src/}&downloaded='+have_download_resume+'"/}'
						}).off('loadComplete').on('loadComplete',function(){
							this.oneCloseEvent('#btnReportSubmit');
						}).show();
					} else {
						showModel('info', result.error);
					}
				});
		}
	})
});

hbjs.use('cqjob.fancybox-thumbs', function($) {
	$.each(fancboxid, function(index, val) {
		$('.fancybox-thumbs' + val).fancybox({
			closeBtn  : true,
			arrows    : true,
			title : function(current) {
				if ($(this).attr('extension') == 'jpg' || $(this).attr('extension') == 'bmp' 
					|| $(this).attr('extension') == 'gif' || $(this).attr('extension') == 'jpeg' 
					|| $(this).attr('extension') == 'png') {
					return "<a href='"+$(this).attr('href')+"' target='_blank'>查看原图</a>";
				}
				else {
					return "<a href='"+$(this).attr('real_heaf')+"' target='_blank'>下载文件</a>";
				}
			},
			maxWidth : 1000,
			maxHeight : 600,
			helpers : {
				title : {
					type : 'float'
				}
			}
		});	
	});

	$('.my-case').on('click', '.img a', function(e){
		var item   = $(e.currentTarget),
			attrid = item.attr('data-achievementid');
		
		$('.fancybox-thumbs' + attrid).eq(0).trigger('click');
	});
});
</script>
</body>
</html>