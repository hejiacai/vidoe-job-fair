<iframe id="printIframe"  height="0" width="0" frameborder="0"></iframe>
<script type="text/javascript">
	var isDownLoad = false;
	$('.hbTip').tooltip({
	 	selector: "a[data-toggle=tooltip],p[data-toggle=tooltip]",html:true
	});
	Fixed_Box("#divNavFlag",200,0);
	//浏览器的宽度发送改变时重新设置其样式
	$(window).resize(function()
	{
		Fixed_Box("#divNavFlag",200,0);
	}); 
	$(document).ready(function(){
		//查询归属地
		searchAttribution();
		//导航
		$("#divNavFlag").find('a').click(function() {
			var _self = $(this);
			var _hight = $('#divNavFlag').height();
			var flag = _self.attr("k");
			if(flag=='top'){
				$('html,body').animate({ scrollTop: 0 });
				$('#flagWork').addClass('cu');
			}else{
				if(flag=='linkway'){
					scroller(flag, 700 , _hight);
				}else{
					scroller(flag, 700 , _hight);
					_self.addClass('cu').siblings().removeClass('cu');
				}
			}
		});
		
		//获取联系方式
		$('#btnGetLinkway').click(function(){			
			//TODO 判断是否关闭了扣点的提示
			var cookieValue = readCookie('downresumeprompt');
			if (cookieValue && cookieValue.length > 0){
			    $.getJSON('{/get_url rule="/download/getlinkway"/}',{resumeID: {/$resume_id/},companyID: {/$company_id/}}, function(json){
					if (json&&json.error) {
						$.message(json.error, {title:"获取联系方式", icon:'fail'});
						return false;
					}
					$('#linkwayContaner').empty().html(json.contactway);
					searchAttribution();

					//标记分组逻辑
					//go to..
					$.showModal('{/get_url rule="/mark/markdown-resumeid-{/$resume_id/}-operate-down.html"/}',{title:'标记为感兴趣'});
				});
             }else{			
				 $.showModal('{/get_url rule="/download/deductcountprompt"/}',{title:'获取联系方式',onclose:function(){
					if (isDownLoad){
						$.getJSON('{/get_url rule="/download/getlinkway"/}',{resumeID: {/$resume_id/},companyID: {/$company_id/}},function(json){
							if(json&&json.error){
								$.message(json.error,{title:"获取联系方式",icon:'fail'});
								return false;
							}
							$('#linkwayContaner').empty().html(json.contactway);
							searchAttribution();
	
							//标记分组逻辑
							$.showModal('{/get_url rule="/mark/markdown-resumeid-{/$resume_id/}-operate-down.html"/}',{title:'标记为感兴趣'});
						});
					}
				}
				});
             }
		});

		//感兴趣
		$("#btnFav").click(function(){		
			var obj = $(this);
			$.getJSON('{/get_url rule="/fav/fav"/}',{resume_id:{/$resume_id/},company_id:{/$resume_id/},r:Math.random().toString().replace('.','0')},function(json){
				if(json&&json.error){
					$.message(json.error,{icon:'fail'});
					return;
				}
				
				if(json.fav_type == "add"){
					obj.attr("data-original-title","我已感兴趣的，点击取消");
					obj.parent().addClass('mark');
					obj.html("&#xf005;");
					$.showModal('{/get_url rule="/mark/markfav-resumeid-{/$resume_id/}-operate-fav.html"/}',{title:'标记为感兴趣'});
					return;
				}

				if(json.fav_type == "cancel"){
					$.anchorMsg("取消感兴趣成功",{timeout:1});
					obj.attr("data-original-title","标为感兴趣");
					obj.html("&#xf006;");
					obj.parent().removeClass('mark');
					return;
				}
				
			});
		});
	});

	//举报电话号码
	function reportPhone(){
		$.showModal('{/get_url rule="/report/index/" data="resume_id={/$resume_id/}&person_id={/$person_id/}&phone={/$mobile_phone/}"/}',{title:'举报联系方式'});
		return false;
	}

	//查询归属地
	function searchAttribution(){
		//获取号码归属地
		var tel = '{/$mobile_phone/}';
		var regex = /^1[3|4|5|8][0-9]\d{4,8}$/;
		if(!regex.test(tel)){
			$('#phoneAddr').html('（归属地：<b>未知</b>，座机拨打异地手机请加拨“0”）');
		}else{
			 var url = 'http://tcc.taobao.com/cc/json/mobile_tel_segment.htm?tel='+tel;
			  var isIE6 = false;
			  if ($.browser.msie && ($.browser.version == "6.0") && !$.support.style) {
				  isIE6 = true;
				  url = 'https://www.baifubao.com/callback?cmd=1059&callback=phone&phone='+tel;
			  }
			  $.ajax({
				 async:false,
	             type: "post",
