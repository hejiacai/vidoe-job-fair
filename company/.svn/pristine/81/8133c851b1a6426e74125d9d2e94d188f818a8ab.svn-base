<script>
    $('#choice-job-type .body .type-con .type-item').on('click',function () {
        $(this).siblings().removeClass('active').find('i').removeClass('icon-particulars-require')
        $(this).addClass('active').find('i').addClass('icon-particulars-require');
        var jobNum = Number($('#job_num').val())==0 ? 1 : Number($('#job_num').val());
        //console.log(jobNum);
        if($(this).hasClass('pro')){
            $('#is_quality').val(1);
            if('{/$point_job_boutique_left/}'<jobNum && '{/$job_boutique_release/}'>=jobNum){
                $('#choice-job-type .other-info span').hide().siblings('.c1').show();
                $('#choice-job-type .btns .confirm').text('暂存职位');
            }else if('{/$job_boutique_release/}'<jobNum) {
            $('#choice-job-type .other-info span').hide().siblings('.c2').show();
            $('#choice-job-type .btns .confirm').text('暂存职位');
            }else{
            $('#choice-job-type .other-info span').hide();
            $('#choice-job-type .btns .confirm').text('确定')
        }
        }else {
            $('#is_quality').val(0);
            if(Number($(this).find('.simple-count span').text())<jobNum){
                $('#choice-job-type .other-info span').hide().siblings('.simple-info').show();
                $('#choice-job-type .btns .confirm').text('暂存职位')
            }else {
                $('#choice-job-type .other-info span').hide()
                $('#choice-job-type .btns .confirm').text('确定')
            }
        }
    })

</script>
