                                });
                                {//if/}
                                    {//if/}
                            }
                            return false;
                        } else if (json.status == "fail") {
                            //失败
                            $.message("发布职位失败", {title: "操作提示", icon: "fail"});
                        } else if (json.status == "info") {
                            $.message(json.message, {title: "操作提示", icon: "fail"});
                        }
                    } else {
                        //添加数据出现异常
                        $.message(json.error, {title: "系统提示", icon: "fail"});
                    }
                }

                $.ajax({
                    url: form.attr('action'),
