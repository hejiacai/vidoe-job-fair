            function tabSalary(index) {
                x_salaryBg.hide();
                x_salaryBg.eq(index).show();
            }

            function disableSalary(index) {
                var hddSalary1 = $('input[name=hddSalary1]'),
                        hddSalary1End = $('input[name=hddSalary1End]'),
                        hddSalary2 = $('input[name=hddSalary2]'),
