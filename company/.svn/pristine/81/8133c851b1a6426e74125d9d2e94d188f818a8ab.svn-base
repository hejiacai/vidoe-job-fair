        addNetJob: function (object, goonPublish) {
            var automatic = $(".oppenWay").hasClass("oppenWaycut") ? 1 : 0;
            var data = {operate: "publish", /*templateID:template, */goonPublish: goonPublish, hddAutomatic: automatic};
			
            /*
             $(object).submitForm({ beforeSubmit: $.proxy(jobAddValid.form, jobAddValid), data: data, success: jobAdd.successCallBack, clearForm: false});
             */

            if (jobAddValid.form()) {

                var main_jobsort = $('#main_jobsort').val();
                var hddComId = $('#hddComId').val();

                $.ajax({
                    url: "{/get_url rule='/job/SearchSimilarjobs'/}",
                    data: {main_jobsort: main_jobsort, hddComId: hddComId},
                    dataType: 'json',
                    type: 'POST',
                    success: function (data) {
                        if (data.code == 'error') {
                            $.message(data.msg, {title: "系统提示", icon: "fail"});
                            return;
                        } else if (data.code == 'excess') {
                            $.confirm(data.msg, '重复职位确认', function () {
                                frompost();
                            });
                        } else {
                            frompost();
                        }
                    }
                });
            }

            function frompost() {
                $(object).running();
                var form = $('#formjobadd');
                var data = 'operate=publish&goonPublish=' + goonPublish + '&hddAutomatic=' + automatic + '&';
                data += form.serialize();
                var count = 0;

                function successBack(json) {
                    var ismustreply = $(".txtReApplyType:checked").val();
                    $(object).stopRunning();
                    if (json && json.requirst_again) {
                        if (count >= 1) {
                            //添加数据出现异常
                            $.message(json.error, {title: "系统提示", icon: "fail"});
                            return;
                        }

                        $.ajax({
                            url: form.attr('action'),
                            data: data,
                            dataType: 'json',
                            type: 'POST',
                            success: successBack
                        });
                        count++;
                        return;
                    }
                    $(object).stopRunning();
                    if (json && json.status) {

                        if (json.status == "succeed") {
                            //成功
                            if (json.goPublish) {
                                $.anchorMsg("发布职位成功", {
                                    onclose: function () {
                                        window.location.href = '{/get_url rule="/job/add"/}';
                                    }
                                });
                                return false;
                            } else if (json.goPublish == false) {
