        var show_not_use_job = "{/$show_not_use_job/}";
        if (keyword == '输入姓名或简历编号') {
            keyword = "";
        }
        var data = [];
        if (job_id != '')
            data.push("job_id=" + job_id);

        if (marriage_id != '')
            data.push("marriage_id=" + marriage_id);
        if (dutytime_id != '')
            data.push("dutytime_id=" + dutytime_id);
        if (sex_id != '')
            data.push("sex_id=" + sex_id);
        if (years_id2 != '')
            data.push("years_id2=" + years_id2);
