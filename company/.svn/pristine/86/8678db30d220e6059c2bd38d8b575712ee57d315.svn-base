			var chatPhonex = $('#famousePhone').val();
			var pattern = /^[1]\d{10}$/;
			if(chatPhonex == ''){
				$.message("请输入手机号");
			}else if(!pattern.test(chatPhonex)){
				$.message("手机号码格式不正确");
			}else{
                $.getJSON("{/get_url rule='/votesgin/SendAuthCode'/}",{mobile_phone:chatPhonex},function(result){
                    //closeLoading();
                    $.message(result.msg);
                    chatinterval = setInterval(countdown,1000);
                });
			}
		});
		
		var countdown = function(){
		
	        var seconds = $('.cutDown em').html();
	        seconds = parseInt(seconds);
	        if(seconds>0){
	            seconds--;
	            $('.cutDown em').html(seconds);
	            $('#btnSendValidate').hide();
	            $('.cutDown').show();
	        }else{
	            clearInterval(chatinterval);
	            $('#btnSendValidate').show().text('重新发送');
	            $('.cutDown').hide();
	           $('.cutDown em').text("60");
	        }
	    }

