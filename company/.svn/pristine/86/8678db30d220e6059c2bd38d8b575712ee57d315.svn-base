<script type="text/javascript">

    $(window).ready(function(){
        var arr=['intro','flow','award','price','rules','entry'];
        $('#company_calling').calling({hddName:"famouseTradePut", type:'single', isLimit:true, max:1, unLimitedLevel:3});
