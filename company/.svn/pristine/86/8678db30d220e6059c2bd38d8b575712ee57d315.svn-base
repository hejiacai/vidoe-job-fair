        $('.headNav li').on('click',function(){
            $('.headNav li').removeClass('cut')
            $(this).addClass('cut')	
            location.href="#"+arr[$(this).index()];
        })
        $(document).on('scroll',function(){
            if($(document).scrollTop() > 440){
                $('.headNav div').addClass('fixed')
            }else{
                $('.headNav div').removeClass('fixed')
            }
            $.each(arr,function(i,e){
                var min=$('#'+ e).offset().top-100;
                if($(document).scrollTop()>min){
                    $('.headNav li').removeClass('cut').eq(i).addClass('cut')
                }else{
                    return false;
                }
            })
            if($(document).scrollTop() > 500){
            	$('#qrWindow').addClass('fixed')
            }else{
            	$('#qrWindow').removeClass('fixed')
