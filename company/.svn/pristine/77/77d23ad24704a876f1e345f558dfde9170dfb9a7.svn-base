            title : '关注微信公众号',
            isAjax : true,
            width :550
        });
        $('.notAudit').click(function(){
            notAudit.show();
        })
        $('.notAuditTop').click(function(){
            notAuditTop.show();
        })
        $('.toped').click(function(){
            ConfirmBox.timeBomb("该职位已置顶", {name : 'fail', width: 180});
        })
        $('.buyPoint').on('click',function () {
          pay_dialog.show();
        });

        $('#pay_job_check').click(function () {
            pay_job_check.show();
        })


        $('.sendSms').on('click',function () {
            var  job_flag = $(this).attr('data-flag');
            var  url = "{/get_url rule='/part/SendSms'/}/job_flag-"+job_flag;
            $.post("{/get_url rule='/part/SendSms'/}",{job_flag:job_flag,is_ajax:1},function(data){
                if(data.can_total==0){
                    ConfirmBox.timeBomb("该岗位暂无待面试和已录用求职者，邀请面试或已录用后可群发短信", {name : 'fail', width: 560});
                    return;
                }
                if(data.total==0){
                    ConfirmBox.timeBomb("当前岗位待面试和已录用人员已达今日发送上限，明日可继续发送", {name : 'fail', width: 560});
                    return;
                }
                sendSms.setContent({
                    content: url
                }).show();
            },'json');


        });


        $('.setTop').on('click',function () {
            var topurl = "{/get_url rule='/partorder/buyjobtop'/}job_id-"+$(this).attr('data-id');
            setTop.setContent({
                content: topurl
            }).show()
            
        });

        $('.suspendJob').on('click',function () {
            $.post("{/get_url rule='/part/SuspendJob/'/}",{job_flag:$(this).attr('data-flag')},function(data){
                if(!data.status){
                    ConfirmBox.timeBomb(data.msg, {name : 'fail', width: 200});
                    return;
                }
                ConfirmBox.timeBomb(data.msg, {name : 'success', width: 150});
                setTimeout("window.location.reload()",3000);
            },'json');
        });


        $('.EndJob').on('click',function () {
            var job_falg = $(this).attr('data-flag');
            ConfirmBox.confirm('是否确认结束岗位？如该岗位有待处理的简历，系统将默认处理成不合适，您可在不合适简历中查询该简历。', '职位停招', function(){
                var self = this;
                $.post("{/get_url rule='/partjob/EndJob/'/}",{job_flag:job_falg},function(data){
                    self.hide();
                    if(data.error){
                        ConfirmBox.timeBomb(data.error, {name : 'fail', width: fontSize * data.error.length + pWidth});
                        return;
                    }
                    ConfirmBox.timeBomb(data.success, {name : 'success', width: fontSize * data.success.length + pWidth});
                    setTimeout("window.location.reload()",3000);

                },'json');
            },null,{width:440, close : 'x'});

        });

        $('.refresh').on('click',function () {
            $.post("{/get_url rule='/part/refreshJob/'/}",{job_flag:$(this).attr('data-flag')},function(data){
                if(!data.status){
                    ConfirmBox.timeBomb(data.msg, {name : 'fail', width: 250,timeout:3000});
                    return;
                }
                ConfirmBox.timeBomb(data.msg, {name : 'success', width: 320,timeout:3000});
                setTimeout("window.location.reload()",3000);

            },'json');
        });

        var indexObj = {
            init:function(){
                if($(".tpartList-list .tpartList-item").length==0){
                    $("#no_data_show").show();
                }

                $(".setCompanyAgency").click(function(){
                    var tourl = "{/get_url rule='/part/setCompanyAgency/callBackPage-index'/}";
                    setCompanyAgency.setContent({
                        isOverflow: true,
                        content: tourl
                    }).show().on('loadComplete', function(){
                        //this.oneCloseEvent('#subButton');
                        /* 弹窗 */
                        var self = this;
                        this.query("#subButton").on('click', function(){
                            //提交
                            var agency = $("input[name='agency']:checked").val();
                            var params = {agency:agency};
                            $.post('{/get_url rule="/part/setCompanyAgencyDo/"/}', params, function(data) {
                                if(data.error){
                                    ConfirmBox.timeBomb(data.error, {name : 'fail', width: 400});
                                    self.hide();
                                    return;
                                }
                                ConfirmBox.timeBomb(data.success, {name : 'success', width: 200});
                               // indexObj.SelectCallBack();
                                self.hide();
                               // window.location.href = "{/$siteurl.company|replace:'http:':''/}/partjob/jobV2/";
                                pubLimit(1,0);
                            },"json");
                        }).after('', function(){
                            this.query('#subButton').off();

                        })
                    });
                });
//关注微信公众号弹窗
                $('.showWx').click(function () {
                    var html = '<div class="weixin-follow">' +
                            '    <div class="weixin-follow-img">' +
                            '        <img id="twd_url" src="https://mp.weixin.qq.com/cgi-bin/showqrcode?ticket={/$ticket/}" alt="">' +
                            '    </div>' +
                            '    <p>微信扫码关注汇博兼职公众号</p>' +
                            '    <ul class="weixin-follow-list clearfix">' +
                            '        <li><i></i>快捷发布职位</li>' +
                            '        <li><i></i>便捷职位管理</li>' +
                            '        <li><i></i>投递微信处理</li>' +
                            '        <li><i></i>免费群发短信</li>' +
                            '    </ul>' +
                            '    <p class="weixin-follow-text-left">注：汇博兼职服务号专用于兼职投递信息处理，全职招聘请关注汇博招聘服务号。</p>' +
                            '    <a href="javascript:;" class="weixin-follow-jump">跳过</a>' +
                            '</div>';

                    //could show
                    $.ajax({
                        url: "{/$siteurl.company/}/part/IsBindWx",
                        type: "post",
                        data: {},
                        dataType: 'json',
                        success: function (res) {
                            if (res.status) {
                                if (res.is_show) {
                                    if(res.ticket){$('#twd_url').attr('src',"https://mp.weixin.qq.com/cgi-bin/showqrcode?ticket="+res.ticket)}
                                    weixinDialog.show();
                                    var checkStatus = setInterval(function () {
                                        $.ajax({
                                            url: "{/$mjianzhi_url/}/weixin/GetUserStatus",
                                            type: "post",
                                            data: {company_id:"{/$company_id/}"},
                                            dataType: 'jsonp',
                                            jsonp: "jsonpCallback",
                                            success: function (res) {
                                                if(res.status && res.can_go){

                                                    $('.weixin-follow-jump').click();
                                                    clearInterval(checkStatus);
                                                }
                                            }

                                        })
                                    },2500);
                                }else{
                                    $('.weixin-follow-jump').click();
                                }
                            } else {
                                weixinDialog.show();
                                setInterval(function () {
                                    $.ajax({
                                        url: "{/$mjianzhi_url/}/weixin/GetUserStatus",
                                        type: "post",
                                        data: {company_id:"{/$company_id/}"},
                                        dataType: 'jsonp',
                                        jsonp: "jsonpCallback",
                                        success: function (res) {
                                            if(res.status && res.can_go){
                                                $('.weixin-follow-jump').click();
                                            }
                                        }

                                    })
                                },2500);
                            }
                        }
                    })

                    weixinDialog.setContent(html);

                    weixinDialog.query('.weixin-follow-jump').on('click',function(){
                        {/if $is_agency==0 || is_null($is_agency)/}
                        weixinDialog.hide();
                        var tourl = "{/get_url rule='/part/setCompanyAgency/callBackPage-index'/}";
                        setCompanyAgency.setContent({
                            isOverflow: true,
                            content: tourl
                        }).show().on('loadComplete', function(){
                            //this.oneCloseEvent('#subButton');
                            /* 弹窗 */
                            var self = this;
                            this.query("#subButton").on('click', function(){
                                //提交
                                var agency = $("input[name='agency']:checked").val();
                                var params = {agency:agency};
                                $.post('{/get_url rule="/part/setCompanyAgencyDo/"/}', params, function(data) {
                                    if(data.error){
                                        ConfirmBox.timeBomb(data.error, {name : 'fail', width: 400});
                                        self.hide();
                                        return;
                                    }
                                    ConfirmBox.timeBomb(data.success, {name : 'success', width: 200});
                                    //indexObj.SelectCallBack();
                                    self.hide();
//                                    window.location.href = "{/$siteurl.company|replace:'http:':''/}/partjob/jobV2/";
                                    pubLimit(1,0);
                                },"json");
                            }).after('', function(){
                                this.query('#subButton').off();

                            })
                        });
                        {/else/}
//                        window.location.href="{/$siteurl.company|replace:'http:':''/}/partjob/jobV2/";
                        pubLimit(1,0);
                        {//if/}
                        })

                });

            },
            showPart:function () {
              var show_part = cookie.get('show_part');
              if(show_part != 1){
                showPart.show();
              }
              var exp = new Date();
              exp.setTime(exp.getTime() + 30 * 24 * 3600000);
              cookie.set('show_part', '1', {expires:exp});
            }
        };
        indexObj.init();
        indexObj.showPart();

        function pubLimit(type,job_id) {
            weixinDialog.hide();
            pubLimitDo(type,job_id)
        }
    })

    function pubLimitDo(type,job_id) {

        $.post('/part/pubNumLimit',{job_id:job_id,type:type},function (e) {
            if(e.can_pub){

                window.location.href="{/$siteurl.company|replace:'http:':''/}/partjob/jobV2/";
            }
            else{

                //弹窗购买
                buy_job_num.show();
                
            }
        },'json')
    }

</script>

