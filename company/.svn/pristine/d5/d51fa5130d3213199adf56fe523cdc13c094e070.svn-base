        $.validator.addMethod("ageAstrict", function (value, element, params) {
            var ageLower = $(params[0]).val();
            var isNum = /^\d*$/;

            if (!ageLower) return false;
            if (!(isNum.test(ageLower)) || !(isNum.test(value))) {
                return false;
            } else {
                if (parseInt(value) == 0 || parseInt(ageLower) == 0) {
                    return true;
                }
                if (parseInt(value) >= parseInt(ageLower)) {
                    return true;
                }
                return false;
            }
        }, "年龄下限不能小于年龄上限");

        //验证多个Email格式
        $.validator.addMethod("checkingemail", function (value, element) {
            if (value == '' || value == null ||value.length == 0) {
                return true;
            }
            var arrary = value.split(",");
            if (arrary.length > 3) {
                return false;
            }
            for (key in arrary) {
                var flag = Reg.email.test(arrary[key]);
                if (!flag) {
                    return false;
                }
            }
            return true;
        }, "您指定本信息使用单独的联系方式，请输入正确的email");

