{
    filterHTML: function(str)
    {
        if (str == 'undefined' || str == null || str == '' || str.constructor != String)
        {
            return '';
        }
        return str.replace(/<.*?>/g, function(match) { return match.replace('<', '').replace('>', ''); });
    }
});
