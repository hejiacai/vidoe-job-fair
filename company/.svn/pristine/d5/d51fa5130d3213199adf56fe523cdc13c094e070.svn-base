        $.validator.addMethod('comparePrice', function (value, element){
            var matched = inputId.exec(element.name);
            var id = matched[1],
                EndName = matched[2] ? id : id + 'End',
                otherInput = $(inputName.replace('{x}', EndName)),
                otherValue = parseInt(otherInput.val()),
                value = parseInt(value),
                f;

            //校招企业模拟登录不用填薪资
            var allowGraduate = $('#yjs').is(":checked");
            if((value == 0 || isNaN(value)) && (otherValue == 0 || isNaN(otherValue)) && eval('{/$can_pub_no_salary/}') && allowGraduate)
                return true;

            if (matched[2]) {
                f = value >= otherValue && value / 3 <= otherValue;
            } else {
                f = otherValue >= value && otherValue / 3 <= value;
            }
            if (f) {
                otherInput.removeClass('error');
            }
            return f;
        }, '薪资范围不大于3倍差距<span class="tipArr"></span>');

