<body id="body">
<style type="text/css">
section.section{}
.section hgroup{padding:0;zoom:1;background:url({/$siteurl.style/}/img/c/recBj.jpg) repeat-y #fafafa;}
.left{float:left; display:inline; width:391px; min-height:430px;}
.left .lTit{height:53px; line-height:53px; border-bottom:1px solid #e1e1e1; padding-left:17px; color:#666;}
.left .lCon{padding:20px 18px; zoom:1;}
.lCon .bd{}
.lCon .bd .formMod .l{width:60px;}
.lCon .bd .formMod .r{width:285px;}
.lCon .bd .formMod .r .formChb{margin:0;}
.lCon .bd .formMod .r .formChb label{margin-right:21px;_margin-right:17px;#margin-right:17px;}
.lCon .bd .xbMod .r .formChb label{font-size:14px;}
.lCon .bd .formBtn{margin:25px 0 0 65px;}

.right{float:left; display:inline; width:609px; min-height:430px}
.right .noData{padding:150px 0;zoom:1; background:none;}
.right .noData .noDataTit{font-size:20px;}
.right .noData .noDataTit i.hbFntWes{font-size:45px; color:#f9ae0c; margin-right:10px; vertical-align:middle;}
.right .rCon{padding:0 0 30px 0; zoom:1;}
.rCon .perTit{height:53px; border-bottom:1px solid #e1e1e1; padding:0 20px;zoom:1; line-height:53px; color:#999;}
.rCon .perTit b{color:#444; margin:0 7px;}

.rCon .perCon{padding:10px 20px 0 10px; zoom:1;}
.rCon .perCon .perLst{}
.perLst ul li{height:auto; border-bottom:1px solid #efefef; padding:10px;zoom:1; font-size:12px;overflow: hidden;}
.perLst ul li .formChb{margin:5px 12px 0 0;}
.perLst ul li .perPhoto{width:35px; height:44px; border:0; background:url({/$siteurl.style/}/img/common/user35_44.jpg) no-repeat; float:left; display:inline;}
.perLst ul li .perPhoto img{width:35px; height:44px; border:0;}
.perLst ul li .perInfo{float:left; display:inline; padding-left:12px; width: 450px;}
.perLst ul li .perInfo p{color:#999; line-height:22px;}
.perLst ul li .perInfo a:link,.perLst ul li .perInfo a:active{color:#444; font-size:14px;}
.perLst ul li .perInfo a:hover{color:#00aaff;}
.perLst ul li .perInfo em{margin-left:10px;}
.perLst ul li .sta{float:right; display:inline; text-align:right; line-height:44px;}
.perLst ul li .sta p.staIn{}
.perLst ul li .sta p.staInTo{color:#5ba333;}
.perLst ul li.hov{background:#fff;}
.rCon .perCon .allBtn{margin-top:30px; height:30px;}
.rCon .perCon .allBtn .formChb{margin:0 0 0 10px;}
.page{text-align:left; _margin-left:15px;#margin-left:10px;}
.page a{width:25px; height:25px; margin:0 4px;}
.page a:first-child{margin-left:10px;}
